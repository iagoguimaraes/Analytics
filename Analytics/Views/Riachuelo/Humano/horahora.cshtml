@{
    Layout = "~/Views/master.cshtml";
}

<div ng-controller="ctrl-riachuelo-hum-horahora">    
    <div ng-if="dashboard.status == 200">

        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Hora Hora Humano</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>

        <!-- FILTROS -->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">

                        <!--DATA INCIAL-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Data Inicial</label>
                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                            </div>
                        </div>

                        <!--DATA FINAL-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Data Final</label>
                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                            </div>
                        </div>

                        <!--EMPRESA-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Empresa</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.empresas" dados="dashFiltros.empresas" style="width:100%"></select2>
                        </div>

                        <!--CARTEIRA-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Carteira</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.carteiras" dados="dashFiltros.carteiras" style="width:100%"></select2>
                        </div>

                        <!--BANDEIRA-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Bandeira</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.bandeira" dados="dashFiltros.bandeira" style="width:100%"></select2>
                        </div>

                        <!--ATRASOS-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Faixa de Atraso</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.atrasos" dados="dashFiltros.atrasos" style="width:100%"></select2>
                        </div>

                        <!--FAIXA DE VALOR-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Faixa de Valor</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.saldo" dados="dashFiltros.saldo" style="width:100%"></select2>
                        </div>

                        <!--SUPERVISOR-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Supervisores</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisores" dados="dashFiltros.supervisores" style="width:100%"></select2>
                        </div>

                        <!--BASE SAVE-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Segmentação</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.save" dados="dashFiltros.save" style="width:100%"></select2>
                        </div>

                        <!--ORIGEM PROMESSA
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Origem da Promessa</label>
                            <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.origemPromessa" dados="dashFiltros.origemPromessa" style="width:100%"></select2>
                        </div>
                        -->

                        <!--BOTÃO-->
                        <div class="form-group">
                            <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!-- RESUMO INDICADORES -->
        <div class="box box-widget widget-user">
            <div class="widget-user-header bg-black" style="background: url('/Content/Images/Riachuelo/oscar_freire.jpg') center center;">
            </div>
            <div class="widget-user-image">
                <img class="img-circle" src="/Content/Images/Riachuelo/riachuelo.png" height="128" width="128">
            </div>
            <div class="box-footer">
                <div class="row">
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.discado | megaNumber"></h5>
                            <span class="description-text">Discado</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.alo | megaNumber"></h5>
                            <span class="description-text">Atendido</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.contato | megaNumber"></h5>
                            <span class="description-text">Contato</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpc | megaNumber"></h5>
                            <span class="description-text">CPC</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpca | number"></h5>
                            <span class="description-text">CPCA</span>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa | number"></h5>
                            <span class="description-text">Promessa</span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row bg-black-gradient">
                <div class="col-sm-2 col-sm-offset-1 border-right">
                    <div class="description-block">
                        <h5 class="description-header" ng-bind="dashboard.resumo.discado / dashboard.resumo.promessa | number:1"></h5>
                        <span class="description-text">Esforço</span>
                    </div>
                </div>
                <div class="col-sm-2 border-right">
                    <div class="description-block">
                        <h5 class="description-header" ng-bind="dashboard.resumo.alo / dashboard.resumo.discado | percentage: 1"></h5>
                        <span class="description-text">Hit Rate</span>
                    </div>
                </div>
                <div class="col-sm-2 border-right">
                    <div class="description-block">
                        <h5 class="description-header" ng-bind="dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1"></h5>
                        <span class="description-text">Tx Loc</span>
                    </div>
                </div>
                <div class="col-sm-2 border-right">
                    <div class="description-block">
                        <h5 class="description-header" ng-bind="dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1"></h5>
                        <span class="description-text">Tx Apv</span>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="description-block">
                        <h5 class="description-header" ng-bind="dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1"></h5>
                        <span class="description-text">Conversao</span>
                    </div>
                </div>
            </div>

        </div>
    </div>

    <!-- TABELA HORA HORA-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Análise por Hora</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('horahora','Table1')">
                        <i class="fa fa-file-excel-o"></i>
                    </div>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horahora">
                            <thead>
                                <tr>
                                    <th>Hora</th>
                                    <th>Discado</th>
                                    <th>Atendido</th>
                                    <th>Contato</th>
                                    <th>CPC</th>
                                    <th>CPCA</th>
                                    <th>Promessa</th>
                                    <th>R$ Promessa</th>
                                    <th>R$ Contábil</th>
                                    <th>Hit Rate</th>
                                    <th>Tx Loc</th>
                                    <th>Tx Apv</th>
                                    <th>Conversão</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="hora in dashboard.horahora">
                                    <td>{{hora.hora}}</td>
                                    <td>{{hora.discado | number}}</td>
                                    <td>{{hora.alo | number}}</td>
                                    <td>{{hora.contato | number}}</td>
                                    <td>{{hora.cpc | number}}</td>
                                    <td>{{hora.cpca | number}}</td>
                                    <td>{{hora.promessa | number}}</td>
                                    <td>{{hora.vlr_promessa | currency:'R$ '}}</td>
                                    <td>{{hora.vlr_contabil | currency:'R$ '}}</td>
                                    <td>{{hora.alo / hora.discado | percentage:1}}</td>
                                    <td>{{hora.cpc / hora.contato | percentage:1}}</td>
                                    <td>{{hora.cpca / hora.cpc | percentage:1}}</td>
                                    <td>{{hora.promessa / hora.cpca | percentage:1}}</td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Total</th>
                                    <th>{{dashboard.resumo.discado | number}}</th>
                                    <th>{{dashboard.resumo.alo | number}}</th>
                                    <th>{{dashboard.resumo.contato | number}}</th>
                                    <th>{{dashboard.resumo.cpc | number}}</th>
                                    <th>{{dashboard.resumo.cpca | number}}</th>
                                    <th>{{dashboard.resumo.promessa | number}}</th>
                                    <th>{{dashboard.resumo.vlr_promessa | currency:'R$ '}}</th>
                                    <th>{{dashboard.resumo.vlr_contabil | currency:'R$ '}}</th>
                                    <th>{{dashboard.resumo.alo / dashboard.resumo.discado | percentage: 1}}</th>
                                    <th>{{dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1}}</th>
                                    <th>{{dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1}}</th>
                                    <th>{{dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1}}</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--TABELA ATRASO/CARTEIRA-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Análise por Atraso</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('CarteiraAtraso','Table8')">
                        <i class="fa fa-file-excel-o"></i>
                    </div>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.atrasos">
                            <thead>
                                <tr>
                                    <th>Atraso/Produto</th>
                                    <th>Contato</th>
                                    <th>CPC</th>
                                    <th>CPCA</th>
                                    <th>Promessa</th>
                                    <th>R$ Promessa</th>
                                    <th>R$ Contábil</th>
                                    <th>TxLoc</th>
                                    <th>TxApv</th>
                                    <th>Conversão</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="e in dashboard.atrasos | groupBy: 'ds_faixa_meta'">

                                <tr class="text-bold">
                                    <td>
                                        <b>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{e[0].ds_faixa_meta}}
                                        </b>
                                    </td>
                                    <td>{{e | sumByColumn: 'contato'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                    <td>{{e | sumByColumn: 'promessa'| number}}</td>
                                    <td>{{e | sumByColumn: 'vlr_promessa'| currency:'R$ '}}</td>
                                    <td>{{e | sumByColumn: 'vlr_contabil'| currency:'R$ '}}</td>
                                    <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                </tr>

                                <tr ng-repeat="c in e" ng-show="showTbody">
                                    <td>{{c.ds_produto}}</td>
                                    <td>{{c.contato | number}}</td>
                                    <td>{{c.cpc | number}}</td>
                                    <td>{{c.cpca | number}}</td>
                                    <td>{{c.promessa | number}}</td>
                                    <td>{{c.vlr_promessa | currency:'R$ '}}</td>
                                    <td>{{c.vlr_contabil | currency:'R$ '}}</td>
                                    <td>{{c.cpc / c.contato | percentage:1}}</td>
                                    <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                    <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                </tr>

                            </tbody>

                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <!--TABELA EMPRESA/CARTEIRA-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Análise por Empresa/Carteira</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('ProducaoEmpresa','Table2')">
                        <i class="fa fa-file-excel-o"></i>
                    </div>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.carteira">
                            <thead>
                                <tr>
                                    <th>Carteira</th>
                                    <th>Discado</th>
                                    <th>Contato</th>
                                    <th>CPC</th>
                                    <th>CPCA</th>
                                    <th>Promessa</th>
                                    <th>R$ Promessa</th>
                                    <th>R$ Contábil</th>
                                    <th>HitRate</th>
                                    <th>TxLoc</th>
                                    <th>TxApv</th>
                                    <th>Conversão</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="e in dashboard.carteira | groupBy: 'ds_emp_nectar'">

                                <tr class="text-bold">
                                    <td>
                                        <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                        {{e[0].ds_emp_nectar}}
                                    </td>
                                    <td>{{e | sumByColumn: 'discado'| number}}</td>
                                    <td>{{e | sumByColumn: 'contato'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                    <td>{{e | sumByColumn: 'promessa'| number}}</td>
                                    <td>{{e | sumByColumn: 'vlr_promessa'| currency:'R$ '}}</td>
                                    <td>{{e | sumByColumn: 'vlr_contabil'| currency:'R$ '}}</td>
                                    <td>{{e | divideSumColumns: 'contato':'discado' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                </tr>

                                <tr ng-repeat="c in e" ng-show="showTbody">
                                    <td>{{c.ds_carteira}}</td>
                                    <td>{{c.discado | number}}</td>
                                    <td>{{c.contato | number}}</td>
                                    <td>{{c.cpc | number}}</td>
                                    <td>{{c.cpca | number}}</td>
                                    <td>{{c.promessa | number}}</td>
                                    <td>{{c.vlr_promessa | currency:'R$ '}}</td>
                                    <td>{{c.vlr_contabil | currency:'R$ '}}</td>
                                    <td>{{c.contato / c.discado | percentage:1}}</td>
                                    <td>{{c.cpc / c.contato | percentage:1}}</td>
                                    <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                    <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--GRAFICO POR HORA-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Gráfico por Hora</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                </div>
                <div class="card-body">
                    <small class="pull-right">
                        <select ng-model="grafico.indicador" ng-change="carregarGrafico()">
                            <option value="hitrate">Hit Rate</option>
                            <option value="txloc">Tx Loc</option>
                            <option value="txapv">Tx Apv</option>
                            <option value="conversao">Conversão</option>
                            <option value="discado">Discado</option>
                            <option value="alo">Atendido</option>
                            <option value="contato">Contato</option>
                            <option value="cpc">CPC</option>
                            <option value="cpca">CPCA</option>
                            <option value="promessa">Promessa</option>
                        </select>
                    </small>
                    <high-chart series="dashboard.grafico" axis-Categories="dashboard.horas" plot-Options-Line-Data-Label="true"></high-chart>
                </div>
            </div>
        </div>
    </div>

    <!--TABELA EQUIPE/SUPERVISOR-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Análise por Supervisor/Equipe</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('EquipeSupervisor','Table4')">
                        <i class="fa fa-file-excel-o"></i>
                    </div>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.equipe">
                            <thead>
                                <tr>
                                    <th>Supervisor</th>
                                    <th>Contato</th>
                                    <th>CPC</th>
                                    <th>CPCA</th>
                                    <th>Promessa</th>
                                    <th>R$ Promessa</th>
                                    <th>R$ Contábil</th>
                                    <th>TxLoc</th>
                                    <th>TxApv</th>
                                    <th>Conversão</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="e in dashboard.equipe | groupBy: 'ds_supervisor'">

                                <tr class="text-bold">
                                    <td>
                                        <b>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{e[0].ds_supervisor}}
                                        </b>
                                    </td>
                                    <td>{{e | sumByColumn: 'contato'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                    <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                    <td>{{e | sumByColumn: 'promessa'| number}}</td>
                                    <td>{{e | sumByColumn: 'vlr_promessa'| currency:'R$ '}}</td>
                                    <td>{{e | sumByColumn: 'vlr_contabil'| currency:'R$ '}}</td>
                                    <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                    <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                </tr>

                                <tr ng-repeat="c in e" ng-show="showTbody">
                                    <td>{{c.ds_equipe}}</td>
                                    <td>{{c.contato | number}}</td>
                                    <td>{{c.cpc | number}}</td>
                                    <td>{{c.cpca | number}}</td>
                                    <td>{{c.promessa | number}}</td>
                                    <td>{{c.vlr_promessa | currency:'R$ '}}</td>
                                    <td>{{c.vlr_contabil | currency:'R$ '}}</td>
                                    <td>{{c.cpc / c.contato | percentage:1}}</td>
                                    <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                    <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                </tr>

                            </tbody>

                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <!--PRODUÇÃO POR OPERADOR-->
    <div class="row margin-bottom">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-header">
                    <span class="card-header-title">Análise por Operador</span>
                    <div class="card-header-button card-header-expand">
                        <i class="fa fa-expand"></i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('ProducaoOperador','Table3')">
                        <i class="fa fa-file-excel-o"> Operador</i>
                    </div>
                    <div class="card-header-button" ng-click="extrair('ProducaoOcorrencia','Table6')">
                        <i class="fa fa-file-excel-o" alt="Visão Operador/Ocorrencia"> Ocorrencia</i>
                    </div>
                </div>
                <div class="card-body">
                    <d-table class="table table-striped table-bordered" style="width:100%" dados="dashboard.operador" scroll-collapse="true" scroll-y="324px" order="[[3,'desc']]"></d-table>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    angular.module('app').controller('ctrl-riachuelo-hum-horahora', function ($scope, $http, $filter, dadosPagina) {
        dadosPagina.titulo = 'Hora Hora';
        dadosPagina.descricao = 'Riachuelo'
        $scope.showTbody = false;

        $scope.dashboard = {};
        $scope.dashFiltros = {};
        $scope.filtros = {
            dtini: $filter('date')(new Date(), "yyyy-MM-dd"),
            dtfim: $filter('date')(new Date(), "yyyy-MM-dd"),
            empresas: '',
            carteiras: '',
            bandeira: '',
            atrasos: '',
            saldo: '',
            supervisores: '',
            save: '',
            //origemPromessa: '',
        }
        $scope.grafico = { indicador: 'promessa' };

        // carregar os filtros
        $http({
            method: 'GET',
            url: "/api/riachuelo/dashboard/humano/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            $scope.dados = r.status;

            $scope.dashFiltros.empresas = r.data.Table;
            $scope.dashFiltros.carteiras = r.data.Table1;
            $scope.dashFiltros.bandeira = r.data.Table14;
            $scope.dashFiltros.atrasos = r.data.Table13;
            $scope.dashFiltros.saldo = r.data.Table15;
            $scope.dashFiltros.supervisores = r.data.Table3;
            $scope.dashFiltros.save = r.data.Table9;
           // $scope.dashFiltros.origemPromessa = r.data.Table16;

        });

        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/riachuelo/dashboard/humano/horahora",
                data: $scope.filtros,
            }).then(function success(r) {
                $scope.dashboard.status = r.status;
                $scope.dashboard.dados = r.data;

                $scope.dashboard.resumo = r.data.Table[0];
                $scope.dashboard.horahora = r.data.Table1;
                $scope.dashboard.carteira = r.data.Table2;
                $scope.dashboard.operador = r.data.Table3;
                $scope.dashboard.equipe = r.data.Table4;
                $scope.dashboard.atrasos = r.data.Table8;

      
                $scope.carregarGrafico();
            });
        }

        // carrega o grafico de acordo com o indicador escolhido
        $scope.carregarGrafico = function () {
            let dados = $scope.dashboard.horahora;
            $scope.dashboard.horas = dados.map(obj => obj.hora);
            switch ($scope.grafico.indicador) {
                case 'hitrate':
                    $scope.dashboard.grafico = [{ name: 'hitrate', data: dados.map(obj => (obj.alo * 100 / obj.discado).toFixed(1) * 1) }];
                    break;
                case 'txloc':
                    $scope.dashboard.grafico = [{ name: 'txloc', data: dados.map(obj => (obj.cpc * 100 / obj.contato).toFixed(1) * 1) }];
                    break;
                case 'txapv':
                    $scope.dashboard.grafico = [{ name: 'txapv', data: dados.map(obj => (obj.cpca * 100 / obj.cpc).toFixed(1) * 1) }];
                    break;
                case 'conversao':
                    $scope.dashboard.grafico = [{ name: 'conversao', data: dados.map(obj => (obj.promessa * 100 / obj.cpca).toFixed(1) * 1) }];
                    break;
                default:
                    $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
            }

        }

        $scope.carregarDashboard();

        // extrair dados para excel
        $scope.extrair = function (nomeArquivo, tabela) {
            alasql('SELECT * INTO XLSX("' + nomeArquivo + '.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }
        
    });
</script>