                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


<script>
    angular.module('app').controller('ctrl-rchlo-portal', function ($scope, $http, $filter) {

        $scope.filtros = {
            dtini: $filter('date')(new Date(), "yyyy-MM-dd"),
            dtfim: $filter('date')(new Date(), "yyyy-MM-dd"),
            servidores: '',
        };
        $scope.dashFiltros = {
            servidores: [{ id: 1, servidor: 'DIGITAL' }, { id: 3, servidor: 'HUMANO' }]
        };
        $scope.grafico = { categoria: 'hora', indicador: 'acesso' };
        $scope.tabela = { indicador: 'qtd' };
        $scope.dashboard = {};


        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/riachuelo/dashboard/portalnegocia",
                data: $scope.filtros,
            }).then(function success(r) {
                $scope.dashboard.dados = r.data;
                $scope.dashboard.resumo = r.data.Table[0];
                $scope.dashboard.dia = r.data.Table1;
                $scope.dashboard.hora = r.data.Table2;
                $scope.dashboard.motivo = r.data.Table3;
                $scope.dashboard.motivo = [{ name: 'Quantidade', innerSize: '60%', data: r.data.Table3.map(obj => ({ name: obj.motivo_recusa, y: obj.qtd })) }];
                $scope.dashboard.carteira = r.data.Table4;
                $scope.dashboard.wppHora = r.data.Table5;
                $scope.dashboard.wppDia = r.data.Table6;

                $scope.carregarGrafico();

                $scope.dashboard.status = r.status;
            });
        }

        // carrega o grafico de acordo com o indicador escolhido
        $scope.carregarGrafico = function () {
            let dados, categorias;

            if ($scope.grafico.categoria == 'hora') {
                dados = $scope.dashboard.hora;
                categorias = dados.map(obj => obj.hora);
            }
            else if ($scope.grafico.categoria == 'data') {
                dados = $scope.dashboard.dia;
                categorias = dados.map(obj => obj.data);
            }

            $scope.dashboard.categorias = categorias;
            $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
        }

        $scope.carregarDashboard();

        // extrair dados para excel
        $scope.extrair = function (nomeArquivo, tabela) {
            alasql('SELECT * INTO XLSX("' + nomeArquivo + '.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

    });
</script>
