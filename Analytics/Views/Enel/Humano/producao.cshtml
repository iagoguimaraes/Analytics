@{
    Layout = "~/Views/master.cshtml";
}



<div ng-controller="ctrl-enel-producao">
    <div ng-if="dashboard.status == 200">

        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Produção Humano</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>


        <!-- FILTROS -->
        <div class="row margin-bottom">

            <div class="col-xs-12" ng-if="dashFiltros.status === 200">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">

                        <!-- Filtros -->
                        <div class="tab-pane active" id="control-sidebar-filters-tab">
                            <!--DATA INCIAL-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Data Inicial</label>
                                <div class="input-group date">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                                </div>
                            </div>

                            <!--DATA FINAL-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Data Final</label>
                                <div class="input-group date">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                                </div>
                            </div>

                            <!--HORA-->
                            <div class="row">
                                <div class="col-xs-12 col-sm-6">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Inicio</label>
                                        <select class="form-control" ng-model="filtros.horaini">
                                            <option value="7" selected="selected">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22">22h</option>
                                        </select>
                                    </div>
                                </div>


                                <div class="col-xs-12 col-sm-6">
                                    <!--HORA FIM-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Final</label>
                                        <select class="form-control" ng-model="filtros.horafim">
                                            <option value="7">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22" selected="selected">22h</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <!--CARTEIRA-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Carteira</label>
                                <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.score" dados="dashFiltros.carteira" style="width:100%"></select2>
                            </div>

                            <!--DIVISAO-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Divisao</label>
                                <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.divisao" dados="dashFiltros.divisao" style="width:100%"></select2>
                            </div>

                            <!--SUPERVISOR-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Supervisor</label>
                                <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisor" dados="dashFiltros.supervisor" style="width:100%"></select2>
                            </div>

                            <!--EQUIPE-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Equipe</label>
                                <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.equipe" dados="dashFiltros.equipe" style="width:100%"></select2>
                            </div>

                            <!--BOTÃO-->
                            <div class="form-group">
                                <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- DAHSBOARD -->
        <div ng-if="dashboard.status == 200">

            <!-- RESUMO INDICADORES -->
            <div class="row margin-bottom">
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-gray">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.discado | megaNumber"></h3>
                            <p>Discado</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-phone"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-gray-active">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.atendido | megaNumber"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.atendido / dashboard.resumo.discado | percentage: 1"></h4>
                            <p>Atendido</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-volume-control-phone"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.contato | megaNumber"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.contato / dashboard.resumo.atendido | percentage: 1"></h4>
                            <p>Contato</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-headphones"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-red">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.cpc | megaNumber"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1"></h4>
                            <p>CPC</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-male"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.cpca | number"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1"></h4>
                            <p>CPCA</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-street-view"></i>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-green">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.promessanectar | number"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.promessanectar / dashboard.resumo.cpca | percentage: 1"></h4>
                            <p>Promessa Nectar</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-thumbs-o-up"></i>
                        </div>
                    </div>
                </div>

            </div>

            <!-- TABELA DIA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Dia</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table1')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.dia">
                                    <thead>
                                        <tr>
                                            <th>Dia</th>
                                            <th>Agentes</th>
                                            <th>Discado</th>
                                            <th>Atendido</th>
                                            <th>Contato</th>
                                            <th>CPC</th>
                                            <th>CPCA</th>
                                            <th>Promessa Nectar</th>
                                            <th>$ Nectar</th>
                                            <th>Promessa Portal</th>
                                            <th>$ Portal</th>
                                            <th>Hit Rate</th>
                                            <th>Tx Loc</th>
                                            <th>Tx Apv</th>
                                            <th>Conversão</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="dia in dashboard.dia">
                                            <td>{{dia.data}}</td>
                                            <td>{{dia.qtd_pa}}</td>
                                            <td>{{dia.discado | number}}</td>
                                            <td>{{dia.atendido | number}}</td>
                                            <td>{{dia.contato | number}}</td>
                                            <td>{{dia.cpc | number}}</td>
                                            <td>{{dia.cpca | number}}</td>
                                            <td>{{dia.promessanectar | number}}</td>
                                            <td>{{dia.vlr_promessanectar | currency:'R$ '}}</td>
                                            <td>{{dia.promessaportal | number}}</td>
                                            <td>{{dia.vlr_promessaportal | currency:'R$ '}}</td>
                                            <td>{{dia.atendido / dia.discado | percentage:1}}</td>
                                            <td>{{dia.cpc / dia.contato | percentage:1}}</td>
                                            <td>{{dia.cpca / dia.cpc | percentage:1}}</td>
                                            <td>{{dia.promessanectar / dia.cpca | percentage:1}}</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Total</th>
                                            <th>{{dashboard.resumo.qtd_pa | number}}</th>
                                            <th>{{dashboard.resumo.discado | number}}</th>
                                            <th>{{dashboard.resumo.atendido | number}}</th>
                                            <th>{{dashboard.resumo.contato | number}}</th>
                                            <th>{{dashboard.resumo.cpc | number}}</th>
                                            <th>{{dashboard.resumo.cpca | number}}</th>
                                            <th>{{dashboard.resumo.promessanectar | number}}</th>
                                            <th>{{dashboard.resumo.vlr_promessanectar | currency:'R$ '}}</th>
                                            <th>{{dashboard.resumo.promessaportal | number}}</th>
                                            <th>{{dashboard.resumo.vlr_promessaportal | currency:'R$ '}}</th>
                                            <th>{{dashboard.resumo.atendido / dashboard.resumo.discado | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.promessanectar / dashboard.resumo.cpca | percentage: 1}}</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!--TABELA EMPRESA/CARTEIRA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Carteira</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table2')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.carteira">
                                    <thead>
                                        <tr>
                                            <th>Carteira</th>
                                            <th>PA</th>
                                            <th>Discado</th>
                                            <th>Atendido</th>
                                            <th>Contato</th>
                                            <th>CPC</th>
                                            <th>CPCA</th>
                                            <th>Promessa Nectar</th>
                                            <th>$ Nectar</th>
                                            <th>Promessa Portal</th>
                                            <th>$ Portal</th>
                                            <th>HitRate</th>
                                            <th>TxLoc</th>
                                            <th>TxApv</th>
                                            <th>Conversão</th>
                                        </tr>
                                    </thead>
                                    <tbody ng-repeat="e in dashboard.carteira | groupBy: 'carteira'">

                                        <tr class="text-bold">
                                            <td>
                                                <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                                {{e[0].carteira}}
                                            </td>
                                            <td>{{e | sumByColumn: 'qtd_pa'| number}}</td>
                                            <td>{{e | sumByColumn: 'discado'| number}}</td>
                                            <td>{{e | sumByColumn: 'atendido'| number}}</td>
                                            <td>{{e | sumByColumn: 'contato'| number}}</td>
                                            <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                            <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                            <td>{{e | sumByColumn: 'promessanectar'| number}}</td>
                                            <td>{{e | sumByColumn: 'vlr_promessanectar'| currency:'R$ '}}</td>
                                            <td>{{e | sumByColumn: 'promessaportal'| number}}</td>
                                            <td>{{e | sumByColumn: 'vlr_promessaportal'| currency:'R$ '}}</td>
                                            <td>{{e | divideSumColumns: 'atendido':'discado' | percentage:1}}</td>
                                            <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                            <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                            <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                        </tr>

                                        <tr ng-repeat="c in e" ng-show="showTbody">
                                            <td>{{c.divisao}}</td>
                                            <td></td>
                                            <td>{{c.discado | number}}</td>
                                            <td>{{c.atendido | number}}</td>
                                            <td>{{c.contato | number}}</td>
                                            <td>{{c.cpc | number}}</td>
                                            <td>{{c.cpca | number}}</td>
                                            <td>{{c.promessanectar | number}}</td>
                                            <td>{{c.vlr_promessanectar | currency:'R$ '}}</td>
                                            <td>{{c.promessaportal | number}}</td>
                                            <td>{{c.vlr_promessaportal | currency:'R$ '}}</td>
                                            <td>{{c.atendido / c.discado | percentage:1}}</td>
                                            <td>{{c.cpc / c.contato | percentage:1}}</td>
                                            <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                            <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                        </tr>

                                    </tbody>

                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!--GRAFICO POR DIA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Gráfico por Dia</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>

                            <small class="pull-right">
                                <select ng-model="grafico.indicador" ng-change="carregarGrafico()">
                                    <option value="hitrate">Hit Rate</option>
                                    <option value="txloc">Tx Loc</option>
                                    <option value="txapv">Tx Apv</option>
                                    <option value="conversao">Conversão</option>
                                    <option value="discado">Discado</option>
                                    <option value="atendido">Atendido</option>
                                    <option value="contato">Contato</option>
                                    <option value="cpc">CPC</option>
                                    <option value="cpca">CPCA</option>
                                    <option value="promessanectar">Promessa Nectar</option>
                                    <option value="vlr_promessanectar">Valor Promessa Nectar</option>
                                    <option value="promessaportal">Promessa Portal</option>
                                    <option value="vlr_promessaportal">Valor Promessa Portal</option>
                                </select>
                            </small>
                        </div>
                        <div class="card-body">
                            <high-chart series="dashboard.grafico" axis-Categories="dashboard.dias" plot-Options-Line-Data-Label="true"></high-chart>
                        </div>
                    </div>
                </div>
            </div>

            <!--TABELA SUPERVISOR/EQUIPE-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Supervisor/Equipe</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table3')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.supervisor">
                                    <thead>
                                        <tr>
                                            <th>Supervisor/Equipe</th>
                                            <th>Contato</th>
                                            <th>CPC</th>
                                            <th>CPCA</th>
                                            <th>Promessa Nectar</th>
                                            <th>$ Nectar</th>
                                            <th>Promessa Portal</th>
                                            <th>$ Portal</th>
                                            <th>TxLoc</th>
                                            <th>TxApv</th>
                                            <th>Conversão</th>
                                        </tr>
                                    </thead>
                                    <tbody ng-repeat="e in dashboard.supervisor | groupBy: 'supervisor'">
                                        <tr class="text-bold">
                                            <td>
                                                <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                                {{e[0].supervisor}}
                                            </td>
                                            <td>{{e | sumByColumn: 'contato'| number}}</td>
                                            <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                            <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                            <td>{{e | sumByColumn: 'promessanectar'| number}}</td>
                                            <td>{{e | sumByColumn: 'vlr_promessanectar' | currency: 'R$ ' }}</td>
                                            <td>{{e | sumByColumn: 'promessaportal'| number}}</td>
                                            <td>{{e | sumByColumn: 'vlr_promessaportal' | currency: 'R$ ' }}</td>
                                            <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                            <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                            <td>{{e | divideSumColumns: 'promessanectar':'cpca' | percentage:1}}</td>
                                        </tr>

                                        <tr ng-repeat="c in e" ng-show="showTbody">
                                            <td>{{c.equipe}}</td>
                                            <td>{{c.contato | number}}</td>
                                            <td>{{c.cpc | number}}</td>
                                            <td>{{c.cpca | number}}</td>
                                            <td>{{c.promessanectar | number}}</td>
                                            <td>{{c.vlr_promessanectar | currency: 'R$ '}}</td>
                                            <td>{{c.promessaportal | number}}</td>
                                            <td>{{c.vlr_promessaportal | currency: 'R$ '}}</td>
                                            <td>{{c.cpc / c.contato | percentage:1}}</td>
                                            <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                            <td>{{c.promessanectar / c.cpca | percentage:1}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!--PRODUÇÃO POR OPERADOR-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Operador</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table4')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <d-table class="table table-striped table-bordered" style="width:100%" dados="dashboard.operador" scroll-collapse="true" scroll-y="324px" order="[[4,'desc']]"></d-table>
                        </div>
                    </div>
                </div>
            </div>

            <!--FINALIZÇÕES DO DISCADOR-->
            <div class="row margin-bottom">
                <div class="col-xs-12 col-lg-6">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Finalizações do Discador</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table5')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                            <small class="pull-right">
                                <select ng-change="carregarDiagramaFila()" ng-model="diagramafila.fila">
                                    <option value="" selected="selected"> Selecione</option>
                                    <option ng-repeat="cc in dashFiltros.fila" value="{{cc.id_fila}}">{{cc.ds_fila}}</option>
                                </select>
                            </small>
                        </div>
                        <div class="card-body">
                            <high-chart series="dashboard.discador" type="pie" style="height:350px" tooltip-Point-Format="{point.y} (<b>{point.percentage:.1f}%</b>)"></high-chart>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-lg-6">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Fila</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table7')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.fila">
                                    <thead>
                                        <tr>
                                            <th>Fila</th>
                                            <th>Agentes</th>
                                            <th>Discado</th>
                                            <th>Atendido</th>
                                            <th>Hit Rate</th>
                                            <th>Ocupação</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="f in dashboard.fila">
                                            <td>{{f.fila}}</td>
                                            <td>{{f.qtd_pa}}</td>
                                            <td>{{f.discado | number}}</td>
                                            <td>{{f.atendido | number}}</td>
                                            <td>{{f.atendido / f.discado | percentage:1}}</td>
                                            <td>{{f.falado / f.logado | percentage:1}}</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Total</th>
                                            <th>{{dashboard.resumofila.qtd_pa | number}}</th>
                                            <th>{{dashboard.resumofila.discado | number}}</th>
                                            <th>{{dashboard.resumofila.atendido | number}}</th>
                                            <th>{{dashboard.resumofila.atendido / dashboard.resumofila.discado | percentage: 1}}</th>
                                            <th>{{dashboard.resumofila.falado / dashboard.resumofila.logado | percentage: 1}}</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- ANÁLISE POR OCORRENCIA -->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title"> Ocorrências no período </span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table7')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <d-table class="table table-striped table-bordered" style="width:100%" scroll-collapse="true" scroll-y="324px" dados="dashboard.ocorrencia" paging="false" order="[[5,'desc', 4, 'desc']]"></d-table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<script>

    angular.module('app').controller('ctrl-enel-producao', function ($scope, $http, $filter, $interval, dadosPagina) {
        dadosPagina.titulo = 'Produção';
        dadosPagina.descricao = 'Enel'

        // filtros iniciais
        let newdate = new Date();
        let primeiroDiaMes = new Date(newdate.getFullYear(), newdate.getMonth(), 1);

        $scope.filtros = {
            dtini: $filter('date')(primeiroDiaMes, "yyyy-MM-dd"),
            dtfim: $filter('date')(newdate, "yyyy-MM-dd"),
            horaini: '7',
            horafim: '22',
            carteira: '',
            divisao: '',
            supervisor: '',
            equipe: '',
        };

        $scope.dashFiltros = {};
        $scope.dashboard = {};
        $scope.grafico = { indicador: 'promessanectar' };
        $scope.diagramafila = { fila: '' };

        // carregar os filtros
        $http({
            method: 'GET',
            url: "/api/enel/humano/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            let data = r.data;
            $scope.dashFiltros.status = r.status;

            $scope.dashFiltros.carteira = r.data.Table;
            $scope.dashFiltros.divisao = r.data.Table1;
            $scope.dashFiltros.supervisor = r.data.Table3;
            $scope.dashFiltros.equipe = r.data.Table4;
            $scope.dashFiltros.fila = r.data.Table2;
        });

        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/enel/humano/producao",
                data: $scope.filtros,
            }).then(function success(r) {
                $scope.dashboard.status = r.status;
                $scope.dashboard.dados = r.data;

                $scope.dashboard.resumo = r.data.Table[0];
                $scope.dashboard.dia = r.data.Table1;
                $scope.dashboard.carteira = r.data.Table2;
                $scope.dashboard.supervisor = r.data.Table3;
                $scope.dashboard.operador = r.data.Table4;

                $scope.carregarDiagramaFila();

                $scope.dashboard.fila = r.data.Table6;
                $scope.dashboard.ocorrencia = r.data.Table7;

                //$scope.dashboard.horasComparativo = r.data.Table5.map(obj => obj.hora);
                //$scope.dashboard.dias = r.data.Table6;

                //$scope.dashboard.graficoComparativo = [
                //    { name: $scope.dashboard.dias[1].name, type: 'spline', color: '#d2d6de', data: r.data.Table5.map(obj => obj[ $scope.dashboard.dias[1].name]) },
                //    { name: $scope.dashboard.dias[3].name, type: 'spline', color: '#dd4b39', data: r.data.Table5.map(obj => obj[$scope.dashboard.dias[3].name]) },
                //    { name: $scope.dashboard.dias[2].name, type: 'spline', color: '#00c0ef', data: r.data.Table5.map(obj => obj[ $scope.dashboard.dias[2].name]) }
                //];


                $scope.carregarGrafico();
            });
        }

        // carregar grafico de diagrama
        $scope.carregarDiagramaFila = function () {

            let data = $scope.dashboard.dados.Table5;
            let id_fila = $scope.diagramafila.fila;

            if ($scope.diagramafila.fila == '') {

                let dados1 = alasql('select ds_ocorrencia, sum(quantidade) quantidade FROM ? group by ds_ocorrencia', [data]);
                $scope.dashboard.discador = [{ name: 'Quantidade', innerSize: '60%', data: dados1.map(obj => ({ name: obj.ds_ocorrencia, y: obj.quantidade })) }];

            }

            let dados2 = alasql('select ds_ocorrencia, sum(quantidade) quantidade FROM ? where id_fila like ("%' + [id_fila] + '%")  group by ds_ocorrencia', [data]);
            $scope.dashboard.discador = [{ name: 'Quantidade', innerSize: '60%', data: dados2.map(obj => ({ name: obj.ds_ocorrencia, y: obj.quantidade })) }];




        }

        // carrega o mapa de acordo com o indicador escolhido
        $scope.carregarGrafico = function () {
            let dados = $scope.dashboard.dia;
            $scope.dashboard.dias = dados.map(obj => obj.data);
            switch ($scope.grafico.indicador) {
                case 'hitrate':
                    $scope.dashboard.grafico = [{ name: 'hitrate', data: dados.map(obj => (obj.atendido * 100 / obj.discado).toFixed(1) * 1) }];
                    break;
                case 'txloc':
                    $scope.dashboard.grafico = [{ name: 'txloc', data: dados.map(obj => (obj.cpc * 100 / obj.contato).toFixed(1) * 1) }];
                    break;
                case 'txapv':
                    $scope.dashboard.grafico = [{ name: 'txapv', data: dados.map(obj => (obj.cpca * 100 / obj.cpc).toFixed(1) * 1) }];
                    break;
                case 'conversao':
                    $scope.dashboard.grafico = [{ name: 'conversao', data: dados.map(obj => (obj.promessanectar * 100 / obj.cpca).toFixed(1) * 1) }];
                    break;
                default:
                    $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
            }
        }

        // extrair dados para excel
        $scope.extrair = function (tabela) {
            alasql('SELECT * INTO XLSX("analytics.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

        $scope.carregarDashboard();

    });

</script>
