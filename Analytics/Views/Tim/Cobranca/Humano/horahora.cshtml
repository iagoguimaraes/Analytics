@{
    Layout = "~/Views/master.cshtml";
}


<div ng-controller="ctrl-tim-horahora">
    <div ng-if="dashboard.status == 200">


        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Hora Hora Humano</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>

        <!-- FILTROS -->
        <div class="row margin-bottom" ng-if="dashFiltros.status == 200">
            <div class="col-xs-12">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row margin-bottom">
                            <!--DATAS-->
                            <div class="row">
                                <div class="col-xs-12 col-sm-6">
                                    <!--DATA INCIAL-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Inicial</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-6">
                                    <!--DATA FINAL-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Final</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!--HORAS-->
                            <div class="row">
                                <div class="col-xs-12 col-sm-6">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Inicio</label>
                                        <select class="form-control" ng-model="filtros.horaini">
                                            <option value="7" selected="selected">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22">22h</option>
                                        </select>
                                    </div>
                                </div>


                                <div class="col-xs-12 col-sm-6">
                                    <!--HORA FIM-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Final</label>
                                        <select class="form-control" ng-model="filtros.horafim">
                                            <option value="7">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22" selected="selected">22h</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <!--EMPRESA-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Empresa</label>
                                <select2 style="width: 100%;" class="form-control" data-placeholder="Selecione" ng-model="filtros.empresa" dados="dashFiltros.empresa"></select2>
                            </div>

                            <!--SEGMENTO-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Segmento</label>
                                <select2 style="width: 100%" class="form-control" data-placeholder="Selecione" ng-model="filtros.segmento" dados="dashFiltros.segmento"></select2>
                            </div>

                            <!--ATRASO-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Faixa de Atraso</label>
                                <select2 style="width: 100%" class="form-control" data-placeholder="Selecione" ng-model="filtros.atraso" dados="dashFiltros.atraso"></select2>
                            </div>

                            <!--PRODUTO-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Produto</label>
                                <select2 style="width: 100%" class="form-control" data-placeholder="Selecione" ng-model="filtros.produto" dados="dashFiltros.produto"></select2>
                            </div>

                            <!--SUPERVISOR-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Supervisor</label>
                                <select2 style="width: 100%" class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisor" dados="dashFiltros.supervisor"></select2>
                            </div>

                            <!--EQUIPE-->
                            <div class="form-group">
                                <label class="control-sidebar-subheading">Equipe</label>
                                <select2 style="width: 100%" class="form-control" data-placeholder="Selecione" ng-model="filtros.equipe" dados="dashFiltros.equipe"></select2>
                            </div>
                        </div>

                        <!--BOTÃO-->
                        <div class="form-group">
                            <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="margin-bottom">
            <div class="form-group">                
                    <button type="button" class="btn btn-danger" data-toggle="control-sidebar" ng-click="DownloadClassificacoes()">Classificação Ocorrências</button>                
                
                    <button type="button" class="btn btn-danger" data-toggle="control-sidebar" ng-click="DownloadCarteiras()">De/para Carteiras</button>                
            </div>
        </div>

        <!-- RESUMO INDICADORES -->
        <div class="box box-widget widget-user">
            <div class="widget-user-header bg-black" style="background: url('/Content/Images/Tim/bg.png') center center;">
            </div>
            <div class="widget-user-image">
                <img class="img-circle" src="/Content/Images/Tim/logo.jpg" height="128" width="128">
            </div>
            <div class="box-footer">
                <div class="row" style="background-color:rgba(255, 255, 255, 0.00); color:black; opacity: 0.8">
                    @*<div class="col-sm-2 col-sm-offset-1 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.discado | megaNumber"></h5>
                                <span class="description-text">Discado</span>
                            </div>
                        </div>*@

                    <div class="col-sm-2 border-right">
                        <div class="description-block">

                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.contato | megaNumber"></h5>
                            <span class="description-text">Contato</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpc | megaNumber"></h5>
                            <span class="description-text">CPC</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpca | number"></h5>
                            <span class="description-text">CPCA</span>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa | number"></h5>
                            <span class="description-text">Promessa</span>
                        </div>
                    </div>
                </div>

                <div class="row bg-blue">
                    @*<div class="col-sm-2 col-sm-offset-1 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.contato / dashboard.resumo.discado | percentage: 1"></h5>
                                <span class="description-text">Hit Rate</span>
                            </div>
                        </div>*@

                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.qtd_op | number"></h5>
                            <span class="description-text">Operadores</span>
                        </div>
                    </div>

                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1"></h5>
                            <span class="description-text">Tx Loc</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1"></h5>
                            <span class="description-text">Tx Apv</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1"></h5>
                            <span class="description-text">Conversao</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.vlr_promessa | currency: 'R$ '"></h5>
                            <span class="description-text">Valor Promessa</span>
                        </div>
                    </div>
                </div>

                <div class="row" style="background-color:rgba(255, 255, 255, 0.00); color:black; opacity: 0.8">
                    @*<div class="col-sm-2 col-sm-offset-1 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.discado | megaNumber"></h5>
                                <span class="description-text">Discado</span>
                            </div>
                        </div>*@

                    <div class="col-sm-2 border-right">
                        <div class="description-block">

                        </div>
                    </div>

                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="(dashboard.resumo.contato / dashboard.resumo.qtd_op) | number : 0"></h5>
                            <span class="description-text">Contato OP</span>
                        </div>
                    </div>
                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="(dashboard.resumo.cpc / dashboard.resumo.qtd_op) | number : 0"></h5>
                            <span class="description-text">CPC OP</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="(dashboard.resumo.cpca / dashboard.resumo.qtd_op) | number : 0"></h5>
                            <span class="description-text">CPCA OP</span>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="(dashboard.resumo.promessa / dashboard.resumo.qtd_op) | number : 0"></h5>
                            <span class="description-text">Promessa OP</span>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <!--ANALISE POR HORA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Hora</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table1')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                        <small class="pull-right">
                            <label>
                                <input type="radio" name="graficoHoraExibir" ng-model="graficoHora.exibir" value="true" checked="checked" />
                                Tabela
                            </label>
                            <label>
                                <input type="radio" name="graficoHoraExibir" ng-model="graficoHora.exibir" value="false" />
                                Gráfico
                            </label>

                        </small>
                    </div>
                    <div class="card-body">
                        <!--TABELA-->
                        <div class="table-responsive" ng-show="graficoHora.exibir == 'true'">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horahora">
                                <thead>
                                    <tr>
                                        <th>Hora</th>
                                        <th>Operadores</th>
                                        @*<th>Discado</th>*@
                                        <th>Contato</th>
                                        <th>CPC</th>
                                        <th>Alega</th>
                                        <th>CPCA</th>
                                        <th>Promessa</th>
                                        <th>Valor Promessa</th>
                                        @*<th>Hit Rate</th>*@
                                        <th>Tx Loc</th>
                                        <th>Tx Alega</th>
                                        <th>Tx Apv</th>
                                        <th>Conversão</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="hora in dashboard.horahora">
                                        <td>{{hora.hora}}</td>
                                        <td>{{hora.qtd_op}}</td>
                                        @*<td>{{hora.discado | number}}</td>*@
                                        <td>{{hora.contato | number}}</td>
                                        <td>{{hora.cpc | number}}</td>
                                        <td>{{hora.alega | number}}</td>
                                        <td>{{hora.cpca | number}}</td>
                                        <td>{{hora.promessa | number}}</td>
                                        <td>{{hora.vlr_promessa| currency: 'R$ ' }}</td>
                                        @*<td>{{hora.contato / hora.discado | percentage:1}}</td>*@
                                        <td>{{hora.cpc / hora.contato | percentage:1}}</td>
                                        <td>{{hora.alega / hora.cpc | percentage:1}}</td>
                                        <td>{{hora.cpca / hora.cpc | percentage:1}}</td>
                                        <td>{{hora.promessa / hora.cpca | percentage:1}}</td>
                                    </tr>
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>Total</th>
                                        @*<th>{{dashboard.resumo.discado | number}}</th>*@
                                        <th>{{dashboard.resumo.qtd_op | number}}</th>
                                        <th>{{dashboard.resumo.contato | number}}</th>
                                        <th>{{dashboard.resumo.cpc | number}}</th>
                                        <th>{{dashboard.resumo.alega | number}}</th>
                                        <th>{{dashboard.resumo.cpca | number}}</th>
                                        <th>{{dashboard.resumo.promessa | number}}</th>
                                        <th>{{dashboard.resumo.vlr_promessa | currency: 'R$ '}}</th>
                                        @*<th>{{dashboard.resumo.contato / dashboard.resumo.discado | percentage: 1}}</th>*@
                                        <th>{{dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1}}</th>
                                        <th>{{dashboard.resumo.alega / dashboard.resumo.cpc | percentage: 1}}</th>
                                        <th>{{dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1}}</th>
                                        <th>{{dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1}}</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>

                        <!--GRAFICO-->
                        <div ng-show="graficoHora.exibir == 'false'">
                            <span class="pull-right">
                                <select ng-model="graficoHora.indicador" ng-change="carregarGraficoHora()">
                                    @*<option value="hitrate">Hit Rate</option>*@
                                    <option value="txloc">Tx Loc</option>
                                    <option value="txapv">Tx Apv</option>
                                    <option value="conversao">Conversão</option>
                                    @*<option value="qtd_discado">Discado</option>*@
                                    <option value="contato">Contato</option>
                                    <option value="cpc">CPC</option>
                                    <option value="cpca">CPCA</option>
                                    <option value="promessa">Promessa</option>
                                </select>
                            </span>
                            <high-chart series="dashboard.graficoHora" axis-Categories="dashboard.horas" plot-Options-Line-Data-Label="true"></high-chart>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--GRAFICO ALEGA PAGAMENTO E ESTADOS-->
        <div class="row margin-bottom">
            <div class="col-xs-12 col-md-6">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Indice de Alega Pagamento</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table5')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <high-chart class="margin-bottom" series="dashboard.alegaPagamento" axis-Categories="dashboard.horas" type="column"
                                    plot-Options-Column-Stacking="normal" plot-Options-spline-Data-Label="true" y-Axis1-Min="0"
                                    y-Axis1-Max="80" height="410"></high-chart>
                    </div>
                </div>
            </div>
            <!--POR ESTADO-->
            <div class="col-xs-12 col-md-6">
                <div class="card">
                    <div id="info" class="card-header">
                        <span class="card-header-title">Promessa por estado</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table7')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                        <small class="pull-right">
                            <select ng-model="mapa.indicador" ng-change="carregarMapa()">
                                <option value="percentual">Percentual por Estado</option>
                                <option value="promessas">Qtd Promessas</option>
                                <option value="conversao">Conversão</option>
                                <option value="vlr_promessas">Valor Promessas</option>
                            </select>
                        </small>
                    </div>

                    <div class="card-body">
                        <div class="row">
                            <div class="col-xs-12 col-md-4">
                                <div class="small-box bg-blue-active">
                                    <div class="inner">
                                        <p><h5>Qtd - {{dashboard.mapa.estado}}</h5></p>
                                        <h4 ng-bind="dashboard.mapa.promessas | number"></h4>
                                        <h5 ng-bind="(dashboard.mapa.percentual)+' %'"></h5>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12 col-md-4">
                                <div class="small-box bg-green-active">
                                    <div class="inner">
                                        <p><h5>Valor - {{dashboard.mapa.estado}}</h5></p>
                                        <h4 ng-bind="dashboard.mapa.vlr_promessas | currency: 'R$ '"></h4>
                                        <h5 ng-bind="dashboard.mapa.ticket_medio | currency: 'R$ '"></h5>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12 col-md-4">
                                <div class="small-box bg-yellow-active">
                                    <div class="inner">
                                        <h5>Conversão</h5>
                                        <h4 ng-bind="(dashboard.mapa.conversao)+' %'"></h4>
                                        <h5>{{dashboard.mapa.estado}}</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <mapa-brasil style="height: 300px" width="100%" dados="dashboard.estados" ng-change="mudarEstado(idSelecionadoMapa)" ng-model="idSelecionadoMapa"></mapa-brasil>
                    </div>
                </div>
            </div>
        </div>

        <!--TABELA CARTEIRA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por produto</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table2')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.produto">
                                <thead>
                                    <tr>
                                        <th>Segmento / Produto</th>
                                        @*<th>Discado</th>*@
                                        <th>Contato</th>
                                        <th>CPC</th>
                                        <th>CPCA</th>
                                        <th>Promessa</th>
                                        <th>Valor Promessa</th>
                                        @*<th>HitRate</th>*@
                                        <th>TxLoc</th>
                                        <th>TxApv</th>
                                        <th>Conversão</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="e in dashboard.produto | groupBy: 'ds_produto'">

                                    <tr class="text-bold">
                                        <td>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{e[0].ds_produto}}
                                        </td>
                                        @*<td>{{e | sumByColumn: 'discado'| number}}</td>*@
                                        <td>{{e | sumByColumn: 'contato'| number}}</td>
                                        <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                        <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                        <td>{{e | sumByColumn: 'promessa'| number}}</td>
                                        <td>{{e | sumByColumn: 'vlr_promessa'| currency: 'R$ '}}</td>
                                        @*<td>{{e | divideSumColumns: 'contato':'discado' | percentage:1}}</td>*@
                                        <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                        <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                        <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                    </tr>

                                    <tr ng-repeat="c in e" ng-show="showTbody">
                                        <td>{{c.ds_aging}}</td>
                                        @*<td>{{c.discado | number}}</td>*@
                                        <td>{{c.contato | number}}</td>
                                        <td>{{c.cpc | number}}</td>
                                        <td>{{c.cpca | number}}</td>
                                        <td>{{c.promessa | number}}</td>
                                        <td>{{c.vlr_promessa| currency: 'R$ '}}</td>
                                        @*<td>{{c.contato / c.discado | percentage:1}}</td>*@
                                        <td>{{c.cpc / c.contato | percentage:1}}</td>
                                        <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                        <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                    </tr>

                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--TABELA SUPERVISOR/EQUIPE-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Supervisor/Equipe</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table3')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.supervisor">
                                <thead>
                                    <tr>
                                        <th>Supervisor/Equipe</th>
                                        <th>Contato</th>
                                        <th>CPC</th>
                                        <th>CPCA</th>
                                        <th>Promessa</th>
                                        <th>Valor Promessa</th>
                                        <th>TxLoc</th>
                                        <th>TxApv</th>
                                        <th>Conversão</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="e in dashboard.supervisor | groupBy: 'ds_supervisor'">

                                    <tr class="text-bold">
                                        <td>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{e[0].ds_supervisor}}
                                        </td>
                                        <td>{{e | sumByColumn: 'contato'| number}}</td>
                                        <td>{{e | sumByColumn: 'cpc'| number}}</td>
                                        <td>{{e | sumByColumn: 'cpca'| number}}</td>
                                        <td>{{e | sumByColumn: 'promessa'| number}}</td>
                                        <td>{{e | sumByColumn: 'vlr_promessa' | currency: 'R$ ' }}</td>
                                        <td>{{e | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                        <td>{{e | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                        <td>{{e | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                    </tr>

                                    <tr ng-repeat="c in e" ng-show="showTbody">
                                        <td>{{c.ds_equipe}}</td>
                                        <td>{{c.contato | number}}</td>
                                        <td>{{c.cpc | number}}</td>
                                        <td>{{c.cpca | number}}</td>
                                        <td>{{c.promessa | number}}</td>
                                        <td>{{c.vlr_promessa | currency: 'R$ '}}</td>
                                        <td>{{c.cpc / c.contato | percentage:1}}</td>
                                        <td>{{c.cpca / c.cpc | percentage:1}}</td>
                                        <td>{{c.promessa / c.cpca | percentage:1}}</td>
                                    </tr>

                                </tbody>

                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--PRODUÇÃO POR OPERADOR-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Operador</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table4')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <d-table class="table table-striped table-bordered" style="width:100%" dados="dashboard.operador" scroll-collapse="true" scroll-y="324px" order="[[4,'desc']]"></d-table>
                    </div>
                </div>
            </div>
        </div>

        <!--TABULACOES-->
        <div class="row">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Tabulações</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table2')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <d-table class="table table-striped table-bordered" style="width:100%" dados="dashboard.tabulacao" paging="false" order="[[2,'desc']]"></d-table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>


<script>
    angular.module('app').controller('ctrl-tim-horahora', function ($scope, $http, $filter) {
        $scope.showTbody = false;

        $scope.filtros = {
            dtini: $filter('date')(new Date(), "yyyy-MM-dd"),
            dtfim: $filter('date')(new Date(), "yyyy-MM-dd"),
            horaini: '7',
            horafim: '22',
            empresa: '',
            segmento: '',
            atraso: '',
            produto: '',
            equipe: '',
            supervisor: '',
        }


        $scope.mapa = {
            indicador: 'percentual'
        };

        $scope.idSelecionadoMapa = '';
        $scope.graficoHora = { indicador: 'promessa', exibir: 'true' };

        $scope.dashFiltros = {};
        $scope.dashboard = {};


        // carregar os filtros
        $http({
            method: 'GET',
            url: "/api/tim/dashboard/humano/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            $scope.dashFiltros.status = r.status;

            $scope.dashFiltros.empresa = r.data.Table;
            $scope.dashFiltros.segmento = r.data.Table1;
            $scope.dashFiltros.atraso = r.data.Table2;
            $scope.dashFiltros.produto = r.data.Table9;
            $scope.dashFiltros.supervisor = r.data.Table3;
            $scope.dashFiltros.equipe = r.data.Table4;
        });
        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/tim/dashboard/humano/horahora",
                data: $scope.filtros,
            }).then(function success(r) {
                $scope.dashboard.status = r.status;
                $scope.dashboard.dados = r.data;

                $scope.dashboard.resumo = r.data.Table[0];
                $scope.dashboard.horahora = r.data.Table1;
                $scope.dashboard.produto = r.data.Table2;
                $scope.dashboard.supervisor = r.data.Table3;
                $scope.dashboard.operador = r.data.Table4;
                $scope.dashboard.tabulacao = r.data.Table6;

                $scope.dashboard.horas = r.data.Table1.map(obj => obj.hora);

                $scope.dashboard.alegaPagamento = [
                    { name: 'CPC', type: 'column', color: 'rgba(124, 197, 230, 1)', data: r.data.Table5.map(obj => obj.CPC) },
                    { name: 'Alega Pagamento', type: 'area', color: 'rgb(1, 70, 145)', data: r.data.Table5.map(obj => obj.AlegaPagamento) },
                    { name: '% Alega Pagamento', yAxis: 1, type: 'spline', color: 'rgba(232, 2, 39, 1)', data: r.data.Table5.map(obj => (obj.AlegaPagamento * 100 / obj.CPC).toFixed(1) * 1) }
                ];

                $scope.carregarMapa();


                $scope.carregarGraficoHora();
            });
        }


        $scope.mudarEstado = function (id) {

            let dados = alasql('select "BR-" + uf id, ' + $scope.mapa.indicador + ' [value], uf, estado, regiao, percentual, conversao, promessas, vlr_promessas, ticket_medio from ? where id like ("%' + id + '%") order by [value] desc', [$scope.dashboard.dados.Table7])[0];
            $scope.dashboard.mapa = dados;

        }

        // carrega o mapa de acordo com o indicador escolhido
        $scope.carregarMapa = function () {

            let dados = $scope.dashboard.dados.Table7;
            switch ($scope.mapa.indicador) {
                case 'percentual':
                    $scope.dashboard.estados = dados.map(obj => ({ id: obj.id, value: (obj.percentual) }));
                    break;
                case 'conversao':
                    $scope.dashboard.estados = dados.map(obj => ({ id: obj.id, value: (obj.conversao) }));
                    break;
                case 'promessas':
                    $scope.dashboard.estados = dados.map(obj => ({ id: obj.id, value: (obj.promessas) }));
                    break;
                case 'vlr_promessas':
                    $scope.dashboard.estados = dados.map(obj => ({ id: obj.id, value: (obj.vlr_promessas) }));
                    break;
                default:
                    $scope.dashboard.estados = dados.map(obj => ({ id: obj.id, value: obj[$scope.mapa.indicador] }));
            }

            $scope.dashboard.maiorUF = alasql('select id from ? order by [value] desc', [$scope.dashboard.estados])[0];

            $scope.mudarEstado($scope.dashboard.maiorUF);

        }

        // carrega o grafico de acordo com o indicador escolhido
        $scope.carregarGraficoHora = function () {
            let dados = $scope.dashboard.horahora;
            $scope.dashboard.horas = dados.map(obj => obj.hora);

            switch ($scope.graficoHora.indicador) {
                case 'hitrate':
                    $scope.dashboard.graficoHora = [{ name: 'hitrate', data: dados.map(obj => (obj.contato * 100 / obj.discado).toFixed(1) * 1) }];
                    break;
                case 'txloc':
                    $scope.dashboard.graficoHora = [{ name: 'txloc', data: dados.map(obj => (obj.cpc * 100 / obj.contato).toFixed(1) * 1) }];
                    break;
                case 'txapv':
                    $scope.dashboard.graficoHora = [{ name: 'txapv', data: dados.map(obj => (obj.cpca * 100 / obj.cpc).toFixed(1) * 1) }];
                    break;
                case 'conversao':
                    $scope.dashboard.graficoHora = [{ name: 'conversao', data: dados.map(obj => (obj.promessa * 100 / obj.cpca).toFixed(1) * 1) }];
                    break;
                default:
                    $scope.dashboard.graficoHora = [{ name: $scope.graficoHora.indicador, data: dados.map(obj => obj[$scope.graficoHora.indicador]) }];
            }

        }

        $scope.carregarDashboard();

        $scope.DownloadClassificacoes = function () {
            $http({
                method: 'GET',
                url: "/api/tim/dashboard/humano/downloadClassificacao",
                data: $scope.filtros,
                responseType: 'arraybuffer',

            }).then(function success(r) {
                $scope.dashboard.status = r.status;

                var file = new File([r.data], { type: "application/csv" });
                var objectUrl = URL.createObjectURL(file);

                var link = document.createElement('a');
                link.style = "display: none";
                link.href = objectUrl;
                link.download = 'CLASSIFICACAO_ANALYTICS_TIM_' + $filter('date')(new Date(), "yyyyMMdd HH:mm:ss") + '.csv';
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
                window.URL.revokeObjectURL(objectUrl);

            });
        }

        $scope.DownloadCarteiras = function () {
            $http({
                method: 'GET',
                url: "/api/tim/dashboard/humano/downloadCarteira",
                data: $scope.filtros,
                responseType: 'arraybuffer',

            }).then(function success(r) {
                $scope.dashboard.status = r.status;

                var file = new File([r.data], { type: "application/csv" });
                var objectUrl = URL.createObjectURL(file);

                var link = document.createElement('a');
                link.style = "display: none";
                link.href = objectUrl;
                link.download = 'DEPARA_CARTEIRAS_ANALYTICS_TIM_' + $filter('date')(new Date(), "yyyyMMdd HH:mm:ss") + '.csv';
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
                window.URL.revokeObjectURL(objectUrl);

            });
        }

        $scope.extrair = function (tabela) {
            alasql('SELECT * INTO XLSX("analytics.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

    });

</script>


