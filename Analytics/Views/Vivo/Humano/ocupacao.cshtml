@{
    Layout = "~/Views/master.cshtml";
}


<div ng-controller="ctrl-vivo-ocupacao">
    <div ng-if="dashboard.status == 200">

        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Ocupação Operação</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>

        <!-- FILTROS -->
        <div class="row margin-bottom" ng-if="dashFiltros.status == 200">
            <div class="col-xs-12">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>

                    <!--DATAS-->
                    <div class="card-body">
                        <div class="row">
                            <div class="col-xs-12 col-sm-6">
                                <!--DATA INCIAL-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Data Inicial</label>
                                    <div class="input-group date">
                                        <div class="input-group-addon">
                                            <i class="fa fa-calendar"></i>
                                        </div>
                                        <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12 col-sm-6">
                                <!--DATA FINAL-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Data Final</label>
                                    <div class="input-group date">
                                        <div class="input-group-addon">
                                            <i class="fa fa-calendar"></i>
                                        </div>
                                        <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--HORAS-->
                        <div class="row">
                            <div class="col-xs-12 col-sm-6">
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Hora Inicio</label>
                                    <select class="form-control" ng-model="filtros.horaini">
                                        <option value="7" selected="selected">07h</option>
                                        <option value="8">08h</option>
                                        <option value="9">09h</option>
                                        <option value="10">10h</option>
                                        <option value="11">11h</option>
                                        <option value="12">12h</option>
                                        <option value="13">13h</option>
                                        <option value="14">14h</option>
                                        <option value="15">15h</option>
                                        <option value="16">16h</option>
                                        <option value="17">17h</option>
                                        <option value="18">18h</option>
                                        <option value="19">19h</option>
                                        <option value="20">20h</option>
                                        <option value="21">21h</option>
                                        <option value="22">22h</option>
                                    </select>
                                </div>
                            </div>

                            <!--HORA FIM-->
                            <div class="col-xs-12 col-sm-6">
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Hora Final</label>
                                    <select class="form-control" ng-model="filtros.horafim">
                                        <option value="7" selected="selected">07h</option>
                                        <option value="8">08h</option>
                                        <option value="9">09h</option>
                                        <option value="10">10h</option>
                                        <option value="11">11h</option>
                                        <option value="12">12h</option>
                                        <option value="13">13h</option>
                                        <option value="14">14h</option>
                                        <option value="15">15h</option>
                                        <option value="16">16h</option>
                                        <option value="17">17h</option>
                                        <option value="18">18h</option>
                                        <option value="19">19h</option>
                                        <option value="20">20h</option>
                                        <option value="21">21h</option>
                                        <option value="22" selected="selected">22h</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <!--DISCADOR-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Discador</label>
                            <select2 style="width: 100%;" class="form-control" data-placeholder="Selecione" ng-model="filtros.discador" dados="dashFiltros.discador"></select2>
                        </div>

                        <!--SUPERVISOR-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">Supervisor</label>
                            <select2 style="width: 100%;" class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisor" dados="dashFiltros.supervisor"></select2>
                        </div>

                        <!--BOTÃO-->
                        <div class="form-group">
                            <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="row margin-bottom">
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box bg-blue-active">
                    <div class="inner">
                        <p>Méd. Logado Op</p>
                        <h3 ng-bind="dashboard.resumo.logado_op | formatTimer "></h3>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box" style="background-color: rgb(124,181,236); color: white">
                    <div class="inner">
                        <h3 ng-bind="dashboard.resumo.falado_op | formatTimer "></h3>
                        <h4 class="pull-right" ng-bind="dashboard.resumo.ocupacao | percentage: 1 "></h4>
                        <p>Falado Op</p>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box" style="background-color: rgb(67,67,72); color: white">
                    <div class="inner">
                        <h3 ng-bind="dashboard.resumo.disponivel_op | formatTimer "></h3>
                        <h4 class="pull-right" ng-bind="dashboard.resumo.perc_disponivel | percentage: 1 "></h4>
                        <p>Disponível Op</p>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box" style="background-color: rgb(144,237,125); color: white ">
                    <div class="inner">
                        <h3 ng-bind="dashboard.resumo.pausa_op | formatTimer "></h3>
                        <h4 class="pull-right" ng-bind="dashboard.resumo.perc_total_pausa | percentage: 1 "></h4>
                        <p>Pausa Op</p>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box" style="background-color: rgb(153,158,255); color: white">
                    <div class="inner">
                        <h3 ng-bind="dashboard.resumo.aftercall_op | formatTimer "></h3>
                        <h4 class="pull-right" ng-bind="dashboard.resumo.perc_after_call | percentage: 1 "></h4>
                        <p>AfterCall Op</p>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2">
                <div class="small-box" style="background-color: rgb(241,92,128); color: white">
                    <div class="inner">
                        <h3 ng-bind="dashboard.resumo.manual_op | formatTimer "></h3>
                        <h4 class="pull-right" ng-bind="dashboard.resumo.perc_manual | percentage: 1 "></h4>
                        <p>Manual Op</p>
                    </div>
                </div>
            </div>
        </div>

        <!-- QUADROS -->
        <div class="row margin-bottom">
            <!--INDICADORES ABSOLUTOS-->
            <div class="col-xs-12 col-sm-6 col-md-3">
                <div class="row">
                    <div class="col-xs-12 col-sm-6 col-md-6">
                        <div class="small-box bg-gray-active">
                            <div class="inner">
                                <h3 align="center" ng-bind="dashboard.resumo.qtd_op"></h3>
                                <p>Operadores</p>
                            </div>
                            <div class="icon">
                                <i class="fa fa-users"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-6">
                        <div class="small-box bg-gray">
                            <div class="inner">
                                <h3 align="center" ng-bind="dashboard.resumo.qtd_pa | number : 0"></h3>
                                <p>PA's</p>
                            </div>
                            <div class="icon">
                                <i class="fa fa-laptop"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-12">
                        <div class="small-box bg-gray">
                            <div class="inner">
                                <h3 align="center" ng-bind="dashboard.resumo.tma | formatTimer"></h3>
                                <p>TMA</p>
                            </div>
                            <div class="icon">
                                <i class="fa fa-clock-o"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-12">
                        <div class="small-box bg-gray">
                            <div class="inner">
                                <h3 align="center" ng-bind="dashboard.resumo.tme | formatTimer"></h3>
                                <p>TME</p>
                            </div>
                            <div class="icon">
                                <i class="fa fa-clock-o"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-5">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Ocupação por Operador</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table1')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <high-chart series="dashboard.ocupacao" type="pie" height="250" tooltip-Point-Format="{point.y} (<b>{point.percentage:.1f}%</b>)"></high-chart>
                    </div>
                </div>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-4">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Ocupação por Painel</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table2')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <high-chart series="dashboard.painel" type="pie" height="250" tooltip-Point-Format="{point.y} (<b>{point.percentage:.1f}%</b>)"></high-chart>
                    </div>
                </div>
            </div>
        </div>

        <!--ANALISE POR SUPERVISOR-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Supervisor/Equipe</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table3')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <!--TABELA-->
                        <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.supervisor">
                            <thead>
                                <tr>
                                    <th>Supervisor</th>
                                    <th>Operador</th>
                                    <th>Atendidas</th>
                                    <th>TMA</th>
                                    <th>TME</th>
                                    <th>Logado</th>
                                    <th>Offline</th>
                                    <th>Falado</th>
                                    <th>Disponível</th>
                                    <th>Pausas</th>
                                    <th>AfterCall</th>
                                    <th>Manual</th>
                                    <th>% Ocupação</th>
                                    <th>% Pausa</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="c in dashboard.supervisor | groupBy: 'supervisor'">

                                <tr class="text-bold">
                                    <td>
                                        <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                        {{c[0].supervisor}}
                                    </td>
                                    <td>{{c | sumByColumn: 'qtd_op' | number : 0 }}</td>
                                    <td>{{c | sumByColumn: 'qtd_atendidas' | number : 0 }}</td>
                                    <td>{{c | divideSumColumns: 'tmp_falado':'qtd_atendidas' | number : 0 | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_disponivel':'qtd_atendidas' | number : 0 | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_logado':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_offline':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_falado':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_disponivel':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_total_pausa':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_after_call':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_manual':'qtd_op' | formatTimer}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_falado':'tmp_logado' | percentage: 1}}</td>
                                    <td>{{c | divideSumColumns: 'tmp_total_pausa':'tmp_logado' | percentage: 1}}</td>
                                </tr>

                                <tr ng-repeat="p in c" ng-show="showTbody">
                                    <td>{{p.equipe}}</td>
                                    <td>{{p.qtd_op | number}}</td>
                                    <td>{{p.qtd_atendidas | number}}</td>
                                    <td>{{p.tma | formatTimer}}</td>
                                    <td>{{p.tme | formatTimer}}</td>
                                    <td>{{p.logado_op | formatTimer }}</td>
                                    <td>{{p.offline_op | formatTimer }}</td>
                                    <td>{{p.falado_op | formatTimer }}</td>
                                    <td>{{p.disponivel_op | formatTimer }}</td>
                                    <td>{{p.pausa_op | formatTimer }}</td>
                                    <td>{{p.aftercall_op | formatTimer }}</td>
                                    <td>{{p.manual_op | formatTimer }}</td>
                                    <td>{{p.tmp_falado / p.tmp_logado | percentage: 1}}</td>
                                    <td>{{p.tmp_total_pausa / p.tmp_logado | percentage: 1}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <!--ANALISE POR OPERADOR E PA DIA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Aderência por dia</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table4')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                        <small class="pull-right">
                            <label>
                                <input type="radio" name="graficoHoraExibir" ng-model="grafico.exibir" value="true" checked="checked" />
                                Operador
                            </label>
                            <label>
                                <input type="radio" name="graficoHoraExibir" ng-model="grafico.exibir" value="false" />
                                PA
                            </label>

                        </small>
                    </div>
                    <div class="card-body">
                        <!--VISÃO OPERADOR-->
                        <div class="table-responsive" ng-show="grafico.exibir == 'true'">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.dia">
                                <thead>
                                    <tr>
                                        <th>Dia</th>
                                        <th>Operador</th>
                                        <th>Atendidas</th>
                                        <th>TMA</th>
                                        <th>TME</th>
                                        <th>Logado</th>
                                        <th>Offline</th>
                                        <th>Falado</th>
                                        <th>Disponível</th>
                                        <th>Pausa</th>
                                        <th>AfterCall</th>
                                        <th>Manual</th>
                                        <th>% Ocupação</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in dashboard.dia">
                                    <tr>
                                        <td>{{c.data}}</td>
                                        <td>{{c.qtd_op}}</td>
                                        <td>{{c.qtd_atendidas  }}</td>
                                        <td>{{c.tma}}</td>
                                        <td>{{c.tme}}</td>
                                        <td>{{c.logado_op }}</td>
                                        <td>{{c.offline_op }}</td>
                                        <td>{{c.falado_op }}</td>
                                        <td>{{c.disponivel_op }}</td>
                                        <td>{{c.pausa_op }}</td>
                                        <td>{{c.aftercall_op }}</td>
                                        <td>{{c.manual_op }}</td>
                                        <td>{{c.ocupacao}}</td>
                                    </tr>
                                </tbody>

                            </table>
                        </div>
                        <!--VISÃO PA-->
                        <div class="table-responsive" ng-show="grafico.exibir == 'false'">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.dia">
                                <thead>
                                    <tr>
                                        <th>Dia</th>
                                        <th>PA</th>
                                        <th>Atendidas</th>
                                        <th>TMA</th>
                                        <th>TME</th>
                                        <th>Logado</th>
                                        <th>Offline</th>
                                        <th>Falado</th>
                                        <th>Disponível</th>
                                        <th>Pausa</th>
                                        <th>AfterCall</th>
                                        <th>Manual</th>
                                        <th>% Ocupação</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in dashboard.dia">
                                    <tr>
                                        <td>{{c.data}}</td>
                                        <td>{{c.qtd_pa}}</td>
                                        <td>{{c.qtd_atendidas }}</td>
                                        <td>{{c.tma }}</td>
                                        <td>{{c.tme }}</td>
                                        <td>{{c.logado_pa }}</td>
                                        <td>{{c.offline_pa }}</td>
                                        <td>{{c.falado_pa }}</td>
                                        <td>{{c.disponivel_pa }}</td>
                                        <td>{{c.pausa_pa}}</td>
                                        <td>{{c.aftercall_pa }}</td>
                                        <td>{{c.manual_pa}}</td>
                                        <td>{{c.ocupacao}}</td>
                                    </tr>
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--PRODUÇÃO POR OPERADOR-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Tempos por Operador</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table5')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <d-table class="table table-striped table-bordered" style="width:100%" dados="dashboard.operador" scroll-collapse="true" scroll-y="500px" order="[[2,'desc']]"></d-table>
                    </div>
                </div>

            </div>
        </div>

        <!--TEMPOS OPERACIONAIS DIA - HORA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise dos Tempos</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table1')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horaHora">
                                <thead>
                                    <tr>
                                        <th>Dia</th>
                                        <th>PA</th>
                                        <th>Logado Ef.</th>
                                        <th>Logado</th>
                                        <th>Falado</th>
                                        <th>Ocioso</th>
                                        <th>Pausa Total</th>
                                        <th>Pós Atend.</th>
                                        <th>Off-Line</th>
                                        <th>TMA</th>
                                        <th>TMO</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in dashboard.horaHora | groupBy: 'data'">

                                    <tr class="text-bold">
                                        <td>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{c[0].data}}
                                        </td>
                                        <td>{{c | avgColumn: 'qtd_pa' | number : 0 }}</td>
                                        <td>{{c | sumByColumn: 'logadoEF'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'logado'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'falado'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'ocioso'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'pausa_total'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'after_call'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'offline'| formatTimer}}</td>
                                        <td>{{c | avgColumn: 'tma' | number : 0 | formatTimer}}</td>
                                        <td>{{c | avgColumn: 'tmo' | number : 0 | formatTimer}}</td>
                                    </tr>

                                    <tr ng-repeat="p in c" ng-show="showTbody">
                                        <td>{{p.hora}}</td>
                                        <td>{{p.qtd_pa | number}}</td>
                                        <td>{{p.logadoEF | formatTimer}}</td>
                                        <td>{{p.logado | formatTimer }}</td>
                                        <td>{{p.falado | formatTimer }}</td>
                                        <td>{{p.ocioso | formatTimer }}</td>
                                        <td>{{p.pausa_total | formatTimer }}</td>
                                        <td>{{p.after_call | formatTimer }}</td>
                                        <td>{{p.offline | formatTimer}}</td>
                                        <td>{{p.tma | formatTimer}}</td>
                                        <td>{{p.tmo | formatTimer}}</td>
                                    </tr>

                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--TEMPOS PAUSAS DIA - HORA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise de Pausas</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horaHora">
                                <thead>
                                    <tr>
                                        <th>Dia</th>
                                        <th>PA</th>
                                        <th>Pausa NR17</th>
                                        <th>Refeição</th>
                                        <th>Reunião</th>
                                        <th>Pessoal</th>
                                        <th>Descanso</th>
                                        <th>Banheiro</th>
                                        <th>Trab. Interno</th>
                                        <th>FeedBack</th>
                                        <th>Treinamento</th>
                                        <th>Rh</th>
