                                    <tr ng-repeat="p in c" ng-show="showTbody">
                                        <td>{{p.hora}}</td>
                                        <td>{{p.qtd_pa | number}}</td>
                                        <td>{{p.tmp_pausa_NR17 | formatTimer}}</td>
                                        <td>{{p.tmp_refeicao | formatTimer }}</td>
                                        <td>{{p.tmp_Reuniao | formatTimer }}</td>
                                        <td>{{p.tmp_Pessoal | formatTimer }}</td>
                                        <td>{{p.tmp_Descanso | formatTimer }}</td>
                                        <td>{{p.tmp_Banheiro | formatTimer }}</td>
                                        <td>{{p.tmp_TrabInterno | formatTimer }}</td>
                                        <td>{{p.tmp_Feedback | formatTimer}}</td>
                                        <td>{{p.tmp_Treinamento | formatTimer}}</td>
                                        <td>{{p.tmp_rhFinanceiro | formatTimer}}</td>
                                        <td>{{p.tmp_ManutencaoPA | formatTimer}}</td>
                                    </tr>

                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

<script>
    angular.module('app').controller('ctrl-vivo-ocupacao', function ($scope, $http, $filter) {
        // filtros iniciais
        let _dt1 = new Date();
        _dt1.setDate(_dt1.getDate());
        _dt1 = $filter('date')(_dt1, "yyyy-MM-dd");

        $scope.filtros = {
            dtini: _dt1,
            dtfim: _dt1,            
            horaini: '7',
            horafim: '20',
            discador: '',
            supervisor: '',
        };
        $scope.dashFiltros = {};
        $scope.dashboard = {};
        $scope.grafico = { indicador: 'ocupacao', exibir: 'true' };

        
        // carregar os filtros
        $http({
            method: 'GET',
            url: "/api/vivo/humano/dashboard/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            let data = r.data;
            $scope.dashFiltros.status = r.status;
            $scope.dashFiltros.discador = data.Table5;
            $scope.dashFiltros.supervisor = data.Table3;            
        });

        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/vivo/humano/dashboard/ocupacao",
                data: $scope.filtros,
            }).then(function success(r) {
                $scope.dashboard.status = r.status;
                $scope.dashboard.dados = r.data;

                $scope.dashboard.resumo = r.data.Table[0];

                $scope.dashboard.ocupacao = [{ name: 'Quantidade', innerSize: '10%', data: r.data.Table1.map(obj => ({ name: obj.legenda, y: obj.segundos })) }];

                $scope.dashboard.painel = [{ name: 'Quantidade', innerSize: '10%', data: r.data.Table2.map(obj => ({ name: obj.discador, y: obj.medFalado })) }];

                $scope.dashboard.supervisor = r.data.Table3;
                $scope.dashboard.dia = r.data.Table4;
                $scope.dashboard.operador = r.data.Table5;

                //$scope.carregarGrafico();
                //$scope.carregarTempo();
            });
        }


        $scope.carregarGrafico = function () {
            let indicador = $scope.grafico.indicador;
            let dados = $scope.dashboard.dia;

            $scope.dashboard.datas = dados.map(obj => obj.data)
           /* $scope.dashboard.categories = dados.map(r => r.categoria);*/

            let series1;
            switch (indicador) {
                case 'promessa_pa':
                    $scope.dashboard.grafico = [{ name: 'Promessa PA', data: dados.map(obj => obj.promessa_pa) }];

                    break;
                case 'contato_pa':
                    $scope.dashboard.grafico = [{ name: 'Contato PA', data: dados.map(obj => obj.contato_pa) }];

                    break;
                case 'logado':
                    $scope.dashboard.grafico = [{ name: 'Logado PA', data: dados.map(obj => obj.logado) }];

                    break;
                case 'falado':
                    $scope.dashboard.grafico = [{ name: 'Falado PA', data: dados.map(obj => obj.falado) }];

                    break;
                case 'ocioso':
                    $scope.dashboard.grafico = [{ name: 'Ocioso PA', data: dados.map(obj => obj.ocioso) }];

                    break;
                case 'pausa_total':
                    $scope.dashboard.grafico = [{ name: 'Pausa PA', data: dados.map(obj => obj.pausa_total) }];

                    break;
                case 'tma':
                    $scope.dashboard.grafico = [{ name: 'TMA', data: dados.map(obj => obj.pausa_total) }];

                    break;
                case 'tmo':
                    $scope.dashboard.grafico = [{ name: 'TMO', data: dados.map(obj => obj.pausa_total) }];

                    break;
                default:
                    $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
                    
            }

        }
      
        $scope.carregarTempo = function () {

            let dados1 = $scope.dashboard.resumo;
           
            let ocupacao = {
                percent_logado: dados1.logado / dados1.logadoEF,
                percent_pausa: dados1.pausa_total / dados1.logadoEF,
                percent_falado: dados1.falado / dados1.logadoEF,
                percent_aftercall: dados1.after_call / dados1.logadoEF,
                percent_ocioso: dados1.ocioso / dados1.logadoEF,
                percent_offline: dados1.offline / dados1.logadoEF
            }

            
            $scope.dashboard.tmp_lg = { p1: ocupacao.percent_logado };
            $scope.dashboard.tmp_ps = { p1: ocupacao.percent_pausa };
            $scope.dashboard.tmp_fl = { p1: ocupacao.percent_falado };
            $scope.dashboard.tmp_pos = { p1: ocupacao.percent_aftercall };
            $scope.dashboard.tmp_oc = { p1: ocupacao.percent_ocioso };
            $scope.dashboard.tmp_of = { p1: ocupacao.percent_offline };

                       
           
        }

        $scope.carregarDashboard();

        $scope.$watch('grafico.indicador', function (newVal, oldVal) {
            if ($scope.dashboard.status == 200)
            $scope.carregarGrafico();
        });

        $scope.extrair = function (tabela) {
            alasql('SELECT * INTO XLSX("analytics.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

    });
</script>
