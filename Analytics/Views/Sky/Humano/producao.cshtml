@{
    Layout = "~/Views/master.cshtml";
}


<div ng-controller="ctrl-sky-producao">
    <div ng-if="dashboard.status == 200">

        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Produção Humano</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>


        <!-- FILTROS -->
        <div class="row margin-bottom">

            <div class="col-xs-12" ng-if="dashFiltros.status === 200">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">

                        <!-- Filtros -->
                        <div class="tab-pane active" id="control-sidebar-filters-tab">

                            <div class="row">
                                <!--DATA-->
                                <div class="col-xs-12 col-sm-3">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Inicial</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-3">
                                    <!--DATA FINAL-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Final</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-xs-12 col-sm-3">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Inicio</label>
                                        <select class="form-control" ng-model="filtros.horaini">
                                            <option value="6" selected="selected">06h</option>
                                            <option value="7">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22">22h</option>
                                        </select>
                                    </div>
                                </div>


                                <div class="col-xs-12 col-sm-3">
                                    <!--HORA FIM-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Final</label>
                                        <select class="form-control" ng-model="filtros.horafim">
                                            <option value="7">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22" selected="selected">22h</option>
                                        </select>
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-sm-6">
                                    <!--CARTEIRA-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Carteira</label>
                                        <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.carteira" dados="dashFiltros.carteira" style="width:100%"></select2>
                                    </div>

                                    <!--SEGMENTACAO-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Segmentação</label>
                                        <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.segmentacao" dados="dashFiltros.segmentacao" style="width:100%"></select2>
                                    </div>

                                    <!--TENURE-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Tenure</label>
                                        <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.tenure" dados="dashFiltros.tenure" style="width:100%"></select2>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-6">


                                    <!--SUPERVISOR-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Supervisor</label>
                                        <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisor" dados="dashFiltros.supervisor" style="width:100%"></select2>
                                    </div>

                                    <!--OPERADOR-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Operador</label>
                                        <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.operador" dados="dashFiltros.operador" style="width:100%"></select2>
                                    </div>

                                </div>

                            </div>


                            

                            <!--BOTÃO-->
                            <div class="form-group">
                                <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()">Carregar Dados</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- DAHSBOARD -->
        <div ng-if="dashboard.status == 200">

            <div class="row margin-bottom">
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-black">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.discado | megaNumber"></h3>
                            <p>Discado</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-phone"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-aqua-active">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.contato | megaNumber"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.contato / dashboard.resumo.discado | percentage: 1"></h4>
                            <p>Contato</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-headphones"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-red-active">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.cpc | number"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1"></h4>
                            <p>CPC</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-male"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-yellow-active">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.cpca | number"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1"></h4>
                            <p>CPCA</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-street-view"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-green-active">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.promessa | number"></h3>
                            <h4 class="pull-right" ng-bind="dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1"></h4>
                            <p>Promessa</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-thumbs-o-up"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box" style="color:white; background-color:#004592;">
                        <div class="inner">
                            <h3 ng-bind="(dashboard.resumo.tmp_faladoAftercall /  dashboard.resumo.tmp_disponivel) | percentage: 1"></h3>
                            <p>Ocupação Média</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-hourglass-half"></i>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row margin-bottom">
                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-purple">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.qtd_op | number"></h3>
                            <p>Média Operadores</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-users"></i>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <h3 ng-bind="(dashboard.resumo.contato / dashboard.resumo.qtd_op )| number : 0 "></h3>
                            <p>Contato / Op</p>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-red">
                        <div class="inner">
                            <h3 ng-bind="(dashboard.resumo.cpc / dashboard.resumo.qtd_op )| number : 0"></h3>
                            <p>Cpc / Op</p>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <h3 ng-bind="(dashboard.resumo.cpca / dashboard.resumo.qtd_op )| number : 0"></h3>
                            <p>Cpca / Op</p>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-green">
                        <div class="inner">
                            <h3 ng-bind="(dashboard.resumo.promessa / dashboard.resumo.qtd_op )| number : 0"></h3>
                            <p>Promessa / Op</p>
                        </div>
                    </div>
                </div>

                <div class="col-xs-12 col-md-4 col-lg-2">
                    <div class="small-box bg-gray">
                        <div class="inner">
                            <h3 ng-bind="dashboard.resumo.qtd_pago | number: 0"></h3>
                            <h4 class="pull-right" ng-bind="(dashboard.resumo.qtd_pago / dashboard.resumo.qtd_promessa) | percentage: 1"></h4>
                            <p>Efetividade</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-check"></i>
                        </div>
                    </div>
                </div>
            </div>

            <!--ANÁLISE POR DIA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Dia</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table1')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="box-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.dia">
                                        <thead>
                                            <tr>
                                                <th>Dia</th>
                                                <th>Discado</th>
                                                <th>Contato</th>
                                                <th>CPC</th>
                                                <th>CPCA</th>
                                                <th>Promessa</th>
                                                <th>Pro. Cartão</th>
                                                <th>Pro. Migração</th>
                                                <th>Pro. Mg.Cartão</th>
                                                <th>Pro. Parc</th>
                                                <th>% Cartão</th>
                                                <th>% Migração</th>
                                                <th>% Parc.</th>
                                                <th>Tx Loc</th>
                                                <th>Tx Apv</th>
                                                <th>Conversão</th>
                                                <th>% Ocup.</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="dia in dashboard.dia">
                                                <td>{{dia.data}}</td>
                                                <td>{{dia.discado | number}}</td>
                                                <td>{{dia.contato | number}}</td>
                                                <td>{{dia.cpc | number}}</td>
                                                <td>{{dia.cpca | number}}</td>
                                                <td>{{dia.promessa | number}}</td>
                                                <td>{{dia.promessa_cartao | number}}</td>
                                                <td>{{dia.migracao | number}}</td>
                                                <td>{{dia.mgcartao | number}}</td>
                                                <td>{{dia.promessa_parc | number}}</td>
                                                <td>{{dia.promessa_cartao / dia.promessa | percentage:1}}</td>
                                                <td>{{dia.migracao / dia.promessa | percentage:1}}</td>
                                                <td>{{dia.promessa_parc / dia.promessa | percentage:1}}</td>
                                                <td>{{dia.cpc / dia.contato | percentage:1}}</td>
                                                <td>{{dia.cpca / dia.cpc | percentage:1}}</td>
                                                <td>{{dia.promessa / dia.cpca | percentage:1}}</td>
                                                <td>{{dia.tmp_faladoAftercall / dia.tmp_disponivel | percentage:1}}</td>
                                            </tr>
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>Total</th>
                                                <th>{{dashboard.resumo.discado | number}}</th>
                                                <th>{{dashboard.resumo.contato | number}}</th>
                                                <th>{{dashboard.resumo.cpc | number}}</th>
                                                <th>{{dashboard.resumo.cpca | number}}</th>
                                                <th>{{dashboard.resumo.promessa | number}}</th>
                                                <th>{{dashboard.resumo.promessa_cartao | number}}</th>
                                                <th>{{dashboard.resumo.migracao | number}}</th>
                                                <th>{{dashboard.resumo.mgcartao | number}}</th>
                                                <th>{{dashboard.resumo.promessa_parc | number}}</th>
                                                <th>{{dashboard.resumo.promessa_cartao / dashboard.resumo.promessa | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.migracao / dashboard.resumo.promessa | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.promessa_parc / dashboard.resumo.promessa | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1}}</th>
                                                <th>{{dashboard.resumo.tmp_faladoAftercall / dashboard.resumo.tmp_disponivel | percentage:1}}</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!--GRÁFICO INDICADORES POR DIA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Indicadores por Dia</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <small class="pull-right">
                                <select ng-model="grafico.indicador" ng-change="carregarGrafico()">
                                    <option value="ocupacao">% Ocupação</option>
                                    <option value="tmpocioso">% Ocioso</option>
                                    <option value="hitrate">Hit Rate</option>
                                    <option value="txtab">Tx Atendido</option>
                                    <option value="txloc">Tx Loc</option>
                                    <option value="txapv">Tx Apv</option>
                                    <option value="conversao">Conversão</option>
                                    <option value="discado">Discado</option>
                                    <option value="contato">Contato</option>
                                    <option value="cpc">CPC</option>
                                    <option value="cpca">CPCA</option>
                                    <option value="promessa">Promessa</option>
                                    <option value="promessa_cartao">Promessa Cartão</option>
                                    <option value="promessa_parc">Promessa Parcelada</option>
                                    <option value="migracao">Promessa Migração</option>
                                    <option value="mgcartao">Promessa Migração Cartão</option>
                                    <option value="segundos_tma">TMA</option>
                                    <option value="segundos_tme">TME</option>
                                </select>
                            </small>
                        </div>
                        <div class="card-body">
                            <high-chart series="dashboard.grafico" axis-Categories="dashboard.datas" plot-Options-Line-Data-Label="true"></high-chart>
                        </div>
                    </div>
                </div>
            </div>

            <!--GRAFICO VOLUME OCORRÊNCIA POR DIA-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Volume de Ocorrência por Dia</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table7')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                            <small class="pull-right">
                                <label>Ocorrência: </label>
                                <select ng-change="carregarGraficoOcorrenciaDia()" ng-model="grafico.ocorrencia">
                                    <option ng-repeat="cc in dashboard.ocorrenciasList" value="{{cc.id_ocorrencia}}">{{cc.ds_ocorrencia}}</option>
                                </select>
                            </small>
                        </div>
                        <div class="card-body">
                            <high-chart series="dashboard.ocorrenciaDia" axis-Categories="dashboard.datas" plot-Options-Line-Data-Label="true"></high-chart>
                        </div>
                    </div>
                </div>
            </div>

            <!--PRODUÇÃO POR EQUIPE-->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Análise por Supervisor/Equipe</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table3')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.equipe">
                                    <thead>
                                        <tr>
                                            <th>Equipe</th>
                                            <th>Op</th>
                                            <th>Ctt</th>
                                            <th>Cpc</th>
                                            <th>Cpca</th>
                                            <th>Prom.</th>
                                            <th>Tma</th>
                                            <th>% Ocupação</th>
                                            <th>% Pausa</th>
                                            <th>% Manual x Disp.</th>
                                            <th>% Ctt x Att.</th>
                                            <th>% Cart.</th>
                                            <th>% Migr.</th>
                                            <th>% Parc.</th>
                                            <th>% CPC</th>
                                            <th>Conversão</th>
                                        </tr>
                                    </thead>
                                    <tbody ng-repeat="c in dashboard.equipe | groupBy: 'supervisor'">

                                        <tr class="text-bold">
                                            <td>
                                                <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                                {{c[0].supervisor}}
                                            </td>
                                            <td>{{c | sumByColumn: 'qtd_op'| number}}</td>
                                            <td>{{c | sumByColumn: 'contato'| number}}</td>
                                            <td>{{c | sumByColumn: 'cpc' | number}}</td>
                                            <td>{{c | sumByColumn: 'cpca'| number}}</td>
                                            <td>{{c | sumByColumn: 'promessa'| number}}</td>
                                            <td>{{c | divideSumColumns: 'tmp_falado':'contato' | formatTimer}}</td>
                                            <td>{{c | divideSumColumns: 'tmp_faladoAftercall':'tmp_disponivel' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'tmp_pausa':'tmp_logado' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'tmp_manual':'tmp_disponivel' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'atendidas':'contato' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'promessa_cartao':'promessa' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'migracao':'promessa' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'promessa_parc':'promessa' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                            <td>{{c | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                        </tr>

                                        <tr ng-repeat="p in c" ng-show="showTbody">
                                            <td>{{p.equipe}}</td>
                                            <td>{{p.qtd_op | number}}</td>
                                            <td>{{p.contato | number}}</td>
                                            <td>{{p.cpc | number}}</td>
                                            <td>{{p.cpc | number}}</td>
                                            <td>{{p.promessa | number}}</td>
                                            <td>{{p.tmp_falado / p.contato | formatTimer}}</td>
                                            <td>{{p.tmp_faladoAftercall / p.tmp_disponivel | percentage:1}}</td>
                                            <td>{{p.tmp_pausa / p.tmp_logado | percentage:1}}</td>
                                            <td>{{p.tmp_manual / p.tmp_disponivel | percentage:1}}</td>
                                            <td>{{p.atendidas / p.contato | percentage:1}}</td>
                                            <td>{{p.promessa_cartao / p.promessa | percentage:1}}</td>
                                            <td>{{p.migracao / p.promessa | percentage:1}}</td>
                                            <td>{{p.promessa_parc / p.promessa | percentage:1}}</td>
                                            <td>{{p.cpc / p.contato | percentage:1}}</td>
                                            <td>{{p.promessa / p.cpca | percentage:1}}</td>
                                        </tr>

                                    </tbody>

                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!-- ANÁLISE POR OPERADOR -->
            <div class="row margin-bottom">
                <div class="col-xs-12">
                    <div class="card">
                        <div class="card-header">
                            <span class="card-header-title">Consolidado Operador</span>
                            <div class="card-header-button card-header-expand">
                                <i class="fa fa-expand"></i>
                            </div>
                            <div class="card-header-button" ng-click="extrair('Table5')">
                                <i class="fa fa-file-excel-o"></i>
                            </div>
                        </div>
                        <div class="card-body">
                            <d-table class="table table-striped table-bordered" style="width:100%" scroll-collapse="true" scroll-y=450px" dados="dashboard.operador" order="[[6,'desc']]"></d-table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>

    angular.module('app').controller('ctrl-sky-producao', function ($scope, $http, $filter) {
        // filtros iniciais
        let newdate = new Date();


        newdate.setDate(newdate.getDate() - 1);

        let primeiroDiaMes = new Date(newdate.getFullYear(), newdate.getMonth(), 1);

        $scope.filtros = {
            dtini: $filter('date')(primeiroDiaMes, "yyyy-MM-dd"),
            dtfim: $filter('date')(newdate, "yyyy-MM-dd"),
            horaini: '6',
            horafim: '22',
            carteira: '',
            segmentacao: '',
            supervisor: '',
            tenure: '',
            operador: '',
        };
        $scope.dashFiltros = {};
        $scope.dashboard = {};
        $scope.grafico = { indicador: 'promessa', ocorrencia: '' };

        // carregar os filtros
        $http({
            method: 'GET',
            url: "/api/sky/dashboard/humano/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            let data = r.data;
            $scope.dashFiltros.status = r.status;

            $scope.dashFiltros.carteira = r.data.Table;
            $scope.dashFiltros.segmentacao = r.data.Table1;
            $scope.dashFiltros.supervisor = data.Table2;
            $scope.dashFiltros.tenure = data.Table3;
            $scope.dashFiltros.operador = data.Table13;
            $scope.dashFiltros.ocorrencia = data.Table6;
        });

        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/sky/dashboard/humano/producao",
                data: $scope.filtros,
            }).then(function success(r) {
                let data = r.data;
                $scope.dashboard.status = r.status;
                $scope.dashboard.dados = r.data;
                $scope.dashboard.resumo = data.Table[0];
                $scope.dashboard.dia = data.Table1;
                $scope.dashboard.equipe = data.Table2;
                $scope.dashboard.operador = data.Table4;

                $scope.dashboard.ocorrencia = data.Table6;
                $scope.dashboard.ocorrenciasList = alasql('select distinct id_ocorrencia, ds_ocorrencia from ? order by 2', [data.Table6]);

                $scope.carregarGrafico();
                $scope.carregarGraficoOcorrenciaDia();
            });
        }

        // carrega o mapa de acordo com o indicador escolhido
        $scope.carregarGrafico = function () {
            let dados = $scope.dashboard.dia;
            $scope.dashboard.datas = dados.map(obj => obj.data);
            switch ($scope.grafico.indicador) {
                case 'ocupacao':
                    $scope.dashboard.grafico = [{ name: 'ocupação', data: dados.map(obj => (obj.tmp_faladoAftercall * 100 / obj.tmp_disponivel).toFixed(1) * 1) }];
                    break;
                case 'tmpocioso':
                    $scope.dashboard.grafico = [{ name: 'tempo ocioso', data: dados.map(obj => (obj.tmp_ocioso * 100 / obj.tmp_disponivel).toFixed(1) * 1) }];
                    break;
                case 'hitrate':
                    $scope.dashboard.grafico = [{ name: 'hit rate', data: dados.map(obj => (obj.contato * 100 / obj.discado).toFixed(1) * 1) }];
                    break;
                case 'txtab':
                    $scope.dashboard.grafico = [{ name: 'taxa tabulação', data: dados.map(obj => (obj.contato * 100 / obj.transferido).toFixed(1) * 1) }];
                    break;
                case 'txloc':
                    $scope.dashboard.grafico = [{ name: 'taxa localização', data: dados.map(obj => (obj.cpc * 100 / obj.contato).toFixed(1) * 1) }];
                    break;
                case 'txapv':
                    $scope.dashboard.grafico = [{ name: 'taxa aproveitamento', data: dados.map(obj => (obj.cpca * 100 / obj.cpc).toFixed(1) * 1) }];
                    break;
                case 'conversao':
                    $scope.dashboard.grafico = [{ name: 'conversão', data: dados.map(obj => (obj.promessa * 100 / obj.cpca).toFixed(1) * 1) }];
                    break;
                default:
                    $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
            }
        }
        
        $scope.carregarGraficoOcorrenciaDia = function () {
            let idocorrencia = $scope.grafico.ocorrencia;
            let dados = alasql('select data, sum(qtd) qtd from ? group by data order by 1', [$scope.dashboard.ocorrencia.filter(d => d.id_ocorrencia == idocorrencia)]);

            $scope.dashboard.ocorrenciaDia = [{ name: 'Quantidade', type: "line", color: "black", data: dados.map(obj => obj.qtd) }];
        }

        $scope.carregarDashboard();


        // extrair dados para excel
        $scope.extrair = function (tabela) {
            alasql('SELECT * INTO XLSX("analytics.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

    });

</script>