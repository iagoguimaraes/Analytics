<script>
    angular.module('app').controller('ctrl-sky-horahora', function ($scope, $http, $filter) {


        $scope.filtros = {
            dtini: $filter('date')(new Date(), "yyyy-MM-dd"),
            dtfim: $filter('date')(new Date(), "yyyy-MM-dd"),
            carteira: '',
            segmentacao: '',
            supervisor: '',
            tenure: '',
            horaini: '7',
            horafim: '22',
        };
        $scope.dashFiltros = {};
        $scope.dashboard = {};
        $scope.grafico = { indicador: 'promessa' };
        $scope.graficoOco = { classificacao: 'todas', ocorrencia: 'Selecione' };

        //carregar os filtros
        $http({
            method: 'GET',
            url: "/api/sky/dashboard/humano/filtros",
            data: $scope.filtros,
        }).then(function success(r) {
            let data = r.data;
            $scope.dashFiltros.status = r.status;
            $scope.dashFiltros.carteira = data.Table;
            $scope.dashFiltros.segmentacao = data.Table1;
            $scope.dashFiltros.supervisor = data.Table2;
            $scope.dashFiltros.tenure = data.Table3;
            $scope.dashFiltros.ocorrencia = data.Table6;
        });

        // obtem filtros do escopo e carrega os dados do dashboard
        $scope.carregarDashboard = function () {
            $http({
                method: 'POST',
                url: "/api/sky/dashboard/humano/horahora",
                data: $scope.filtros,
            }).then(function success(r) {
                let data = r.data;
                $scope.dashboard.dados = r.data;
                $scope.dashboard.status = r.status;
                $scope.dashboard.resumo = data.Table[0];
                $scope.dashboard.horahora = data.Table1;
                $scope.dashboard.carteira = data.Table2;
                $scope.dashboard.discador = data.Table3;
                $scope.dashboard.equipe = data.Table4;
                $scope.dashboard.operador = data.Table6;
                $scope.dashboard.ocorrencia = data.Table8;
                $scope.dashboard.ocorrenciasList = alasql('select distinct id_ocorrencia, ds_ocorrencia from ? order by 2', [data.Table8]);

                $scope.carregarGrafico();
                $scope.carregarGraficoOcorrencia();
                $scope.carregarGraficoOcorrenciaHora();
            });
        }

        // carrega o grafico de acordo com o indicador escolhido
        $scope.carregarGrafico = function () {
            let dados = $scope.dashboard.horahora;
            $scope.dashboard.horas = dados.map(obj => obj.hora);

            switch ($scope.grafico.indicador) {
                case 'ocupacao':
                    $scope.dashboard.grafico = [{ name: 'ocupação', data: dados.map(obj => ((obj.tmp_falado + obj.tmp_aftercall) * 100 / obj.tmp_disponivel).toFixed(1) * 1) }];
                    break;
                case 'tmpocioso':
                    $scope.dashboard.grafico = [{ name: 'tempo ocioso', data: dados.map(obj => (obj.tmp_ocioso * 100 / obj.tmp_disponivel).toFixed(1) * 1) }];
                    break;
                case 'hitrate':
                    $scope.dashboard.grafico = [{ name: 'hit rate', data: dados.map(obj => (obj.atendidas * 100 / obj.discado).toFixed(1) * 1) }];
                    break;
                case 'txtab':
                    $scope.dashboard.grafico = [{ name: 'taxa tabulação', data: dados.map(obj => (obj.contato * 100 / obj.transferido).toFixed(1) * 1) }];
                    break;
                case 'txloc':
                    $scope.dashboard.grafico = [{ name: 'taxa localização', data: dados.map(obj => (obj.cpc * 100 / obj.contato).toFixed(1) * 1) }];
                    break;
                case 'txapv':
                    $scope.dashboard.grafico = [{ name: 'taxa aproveitamento', data: dados.map(obj => (obj.cpca * 100 / obj.cpc).toFixed(1) * 1) }];
                    break;
                case 'conversao':
                    $scope.dashboard.grafico = [{ name: 'conversão', data: dados.map(obj => (obj.promessa * 100 / obj.cpca).toFixed(1) * 1) }];
                    break;
                default:
                    $scope.dashboard.grafico = [{ name: $scope.grafico.indicador, data: dados.map(obj => obj[$scope.grafico.indicador]) }];
            }
        }

        $scope.carregarGraficoOcorrencia = function () {
            let classificacao = $scope.graficoOco.classificacao;
            let dados = alasql('select ds_ocorrencia, sum(qtd) qtd from ? group by ds_ocorrencia', [$scope.dashboard.ocorrencia.filter(d => d[classificacao] == 1)]);
            $scope.dashboard.ocorrencias = dados.map(obj => obj.ds_ocorrencia);

            $scope.dashboard.graficoOcorrencia = [{ name: 'Quantidade', type: "column", data: dados.map(obj => obj.qtd) }];
        }

        $scope.carregarGraficoOcorrenciaHora = function () {
            let ocorrencia = $scope.graficoOco.ocorrencia;
            let dados = alasql('select hora, sum(qtd) qtd from ? group by hora order by 1', [$scope.dashboard.ocorrencia.filter(d => d.id_ocorrencia == ocorrencia)]);

            $scope.dashboard.ocorrenciaHora = [{ name: 'Quantidade', type: "line", color: "black", data: dados.map(obj => obj.qtd) }];
        }

        // extrair dados para excel
        $scope.extrair = function (tabela) {
            alasql('SELECT * INTO XLSX("analytics.xlsx", {headers: true}) FROM ?', [$scope.dashboard.dados[tabela]]);
        }

        $scope.carregarDashboard();

    });

</script>
