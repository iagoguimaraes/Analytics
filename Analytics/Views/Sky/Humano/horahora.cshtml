@{
    Layout = "~/Views/master.cshtml";
}


<div ng-controller="ctrl-sky-horahora">

    <!-- DAHSBOARD -->
    <div ng-if="dashboard.status === 200">

        <!-- HEADER-->
        <header class="page-header">
            <div class="page-name">Hora Hora Humano</div>
            <div class="page-menu-button" ng-style="{'background-color': credor.cor_primaria, 'color': credor.cor_fonte_primaria}" onclick="toggleDisplayNone('filtros-window')">
                <i class="fa fa-filter"></i>
                FILTROS
            </div>
        </header>

        <!-- FILTROS -->
        <div class="row margin-bottom">

            <div class="col-xs-12" ng-if="dashFiltros.status === 200">
                <div id="filtros-window" class="card display-none">
                    <div class="card-header">
                        <span class="card-header-title">Filtros</span>
                        <div class="card-header-button card-header-close">
                            <i class="fa fa-close"></i>
                        </div>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                    </div>
                    <div class="card-body">

                        <!-- Filtros -->
                        <div class="tab-pane active" id="control-sidebar-filters-tab">
                            <div class="row">
                                <!--DATA-->
                                <div class="col-xs-12 col-sm-3">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Inicial</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtini"></date-picker>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-3">
                                    <!--DATA FINAL-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Data Final</label>
                                        <div class="input-group date">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <date-picker class="form-control pull-right" placeholder="yyyy-mm-dd" ng-model="filtros.dtfim"></date-picker>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-xs-12 col-sm-3">
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Inicio</label>
                                        <select class="form-control" ng-model="filtros.horaini">
                                            <option value="7" selected="selected">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22">22h</option>
                                        </select>
                                    </div>
                                </div>


                                <div class="col-xs-12 col-sm-3">
                                    <!--HORA FIM-->
                                    <div class="form-group">
                                        <label class="control-sidebar-subheading">Hora Final</label>
                                        <select class="form-control" ng-model="filtros.horafim">
                                            <option value="7">07h</option>
                                            <option value="8">08h</option>
                                            <option value="9">09h</option>
                                            <option value="10">10h</option>
                                            <option value="11">11h</option>
                                            <option value="12">12h</option>
                                            <option value="13">13h</option>
                                            <option value="14">14h</option>
                                            <option value="15">15h</option>
                                            <option value="16">16h</option>
                                            <option value="17">17h</option>
                                            <option value="18">18h</option>
                                            <option value="19">19h</option>
                                            <option value="20">20h</option>
                                            <option value="21">21h</option>
                                            <option value="22" selected="selected">22h</option>
                                        </select>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row">

                            <div class="col-xs-12 col-sm-6">
                                <!--CARTEIRA-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Carteira</label>
                                    <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.carteira" dados="dashFiltros.carteira" style="width:100%"></select2>
                                </div>

                                <!--SEGMENTACAO-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Segmentação</label>
                                    <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.segmentacao" dados="dashFiltros.segmentacao" style="width:100%"></select2>
                                </div>
                            </div>

                            <div class="col-xs-12 col-sm-6">

                                <!--TENURE-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Tenure</label>
                                    <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.tenure" dados="dashFiltros.tenure" style="width:100%"></select2>
                                </div>

                                <!--SUPERVISOR-->
                                <div class="form-group">
                                    <label class="control-sidebar-subheading">Supervisor</label>
                                    <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.supervisor" dados="dashFiltros.supervisor" style="width:100%"></select2>
                                </div>

                            </div>

                        </div>


                        <!--BOTÃO-->
                        <div class="form-group">
                            <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <!--BANNER-->
        <div class="box box-widget widget-user">
            <div class="widget-user-header" style="background: url('/Content/Images/Sky/bg.png') center center;">
            </div>

            <!--IMAGEM REDONDA-->
            <div class="widget-user-image">
                <img class="img-circle" src="~/Content/Images/Sky/logo.png" height="128" width="128">
            </div>

            <!--RESUMO DE PRODUÇÃO-->
            <div class="box-footer">

                <div class="row" style="background-color:rgba(255, 255, 255, 0.00); color:black; opacity: 0.8">
                    @*<div class="col-sm-2 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.discado | megaNumber"></h5>
                                <span class="description-text">Discado</span>
                            </div>
                        </div>
                        <div class="col-sm-2 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.transferido | megaNumber"></h5>
                                <span class="description-text">Atendido</span>
                            </div>
                        </div>*@
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.contato | megaNumber"></h5>
                            <span class="description-text">Contato</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpc | megaNumber"></h5>
                            <span class="description-text">CPC</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpca | number"></h5>
                            <span class="description-text">CPCA</span>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa | number"></h5>
                            <span class="description-text">Promessa</span>
                        </div>
                    </div>
                </div>

                <div class="row margin-bottom" style="background-color:#ff0000; color:white;">
                    @*<div class="col-sm-2 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.contato / dashboard.resumo.discado | percentage: 1"></h5>
                                <span class="description-text">Hit Rate</span>
                            </div>
                        </div>
                        <div class="col-sm-2 border-right">
                            <div class="description-block">
                                <h5 class="description-header" ng-bind="dashboard.resumo.contato / dashboard.resumo.transferido | percentage: 1"></h5>
                                <span class="description-text">Tx Tab</span>
                            </div>
                        </div>*@
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1"></h5>
                            <span class="description-text">Tx Loc</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1"></h5>
                            <span class="description-text">Tx Apv</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1"></h5>
                            <span class="description-text">Conversão</span>
                        </div>
                    </div>
                    <div class="col-sm-3 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="(dashboard.resumo.tmp_falado / dashboard.resumo.tmp_disponivel) | percentage: 1"></h5>
                            <span class="description-text">Ocupação</span>
                        </div>
                    </div>
                </div>

                <div class="row" style="background-color:rgba(255, 255, 255, 0.00); color:black; opacity: 0.8">
                    <div class="col-sm-4 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa_cartao | number"></h5>
                            <span class="description-text">Promessa Cartão</span>
                        </div>
                    </div>

                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.migracao | number"></h5>
                            <span class="description-text">Migração</span>
                        </div>
                    </div>

                    <div class="col-sm-2 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.mgcartao | number"></h5>
                            <span class="description-text">Migração Cartão</span>
                        </div>
                    </div>

                    <div class="col-sm-4 border-right">
                        <div class="description-block">
                            <h5 class="description-header" ng-bind="dashboard.resumo.promessa_parc | number"></h5>
                            <span class="description-text">Promessa Parcelada</span>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <!--ANÁLISE POR HORA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Hora</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table1')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="box-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horahora">
                                    <thead>
                                        <tr>
                                            <th>Hora</th>
                                            <th>Ctt</th>
                                            <th>CPC</th>
                                            <th>CPCA</th>
                                            <th>Prom. </th>
                                            <th>Pro. Cartão</th>
                                            <th>Pro. Migração</th>
                                            <th>Pro. Mg. Cartão</th>
                                            <th>Pro. Parc</th>
                                            <th>% Cartão</th>
                                            <th>% Migração</th>
                                            <th>% Parc.</th>
                                            <th>Tx Loc</th>
                                            <th>Tx Apv</th>
                                            <th>Conversão</th>
                                            <th>% Ocup.</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="hora in dashboard.horahora">
                                            <td>{{hora.hora}}</td>
                                            <td>{{hora.contato | number}}</td>
                                            <td>{{hora.cpc | number}}</td>
                                            <td>{{hora.cpca | number}}</td>
                                            <td>{{hora.promessa | number}}</td>
                                            <td>{{hora.promessa_cartao | number}}</td>
                                            <td>{{hora.migracao | number}}</td>
                                            <td>{{hora.mgcartao | number}}</td>
                                            <td>{{hora.promessa_parc | number}}</td>
                                            <td>{{hora.promessa_cartao / hora.promessa | percentage:1}}</td>
                                            <td>{{hora.migracao / hora.promessa | percentage:1}}</td>
                                            <td>{{hora.promessa_parc / hora.promessa | percentage:1}}</td>
                                            <td>{{hora.cpc / hora.contato | percentage:1}}</td>
                                            <td>{{hora.cpca / hora.cpc | percentage:1}}</td>
                                            <td>{{hora.promessa / hora.cpca | percentage:1}}</td>
                                            <td>{{hora.tmp_falado / hora.tmp_disponivel | percentage:1}}</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Total</th>
                                            <th>{{dashboard.resumo.contato | number}}</th>
                                            <th>{{dashboard.resumo.cpc | number}}</th>
                                            <th>{{dashboard.resumo.cpca | number}}</th>
                                            <th>{{dashboard.resumo.promessa | number}}</th>
                                            <th>{{dashboard.resumo.promessa_cartao | number}}</th>
                                            <th>{{dashboard.resumo.migracao | number}}</th>
                                            <th>{{dashboard.resumo.mgcartao | number}}</th>
                                            <th>{{dashboard.resumo.promessa_parc | number}}</th>
                                            <th>{{dashboard.resumo.promessa_cartao / dashboard.resumo.promessa | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.migracao / dashboard.resumo.promessa | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.promessa_parc / dashboard.resumo.promessa | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.cpc / dashboard.resumo.contato | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.cpca / dashboard.resumo.cpc | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.promessa / dashboard.resumo.cpca | percentage: 1}}</th>
                                            <th>{{dashboard.resumo.tmp_falado / dashboard.resumo.tmp_disponivel | percentage:1}}</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--ANÁLISE PERFORMANCE POR HORA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Performance Hora</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table1')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="box-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.horahora">
                                    <thead>
                                        <tr>
                                            <th>Hora</th>
                                            <th>Spin</th>
                                            <th>% Hit Rate</th>
                                            <th>% Tempo Disponivel</th>
                                            <th>% Ocupação</th>
                                            <th>% Pausa</th>
                                            <th>% Tempo Ocioso</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="hora in dashboard.horahora">
                                            <td>{{hora.hora}}</td>
                                            <td>{{hora.spin | number}}</td>
                                            <td>{{hora.atendidas / hora.discado | percentage:1}}</td>
                                            <td>{{hora.tmp_disponivel / hora.tmp_logado | percentage:1}}</td>
                                            <td>{{(hora.tmp_falado + hora.tmp_aftercall) / hora.tmp_disponivel | percentage:1}}</td>
                                            <td>{{hora.tmp_pausa / hora.tmp_logado | percentage:1}}</td>
                                            <td>{{hora.tmp_ocioso / hora.tmp_disponivel | percentage:1}}</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Total</th>
                                            <th>{{dashboard.resumo.spin | number}}</th>
                                            <th>{{dashboard.resumo.atendidas / dashboard.resumo.discado | percentage:1}}</th>
                                            <th>{{dashboard.resumo.tmp_disponivel / dashboard.resumo.tmp_logado | percentage:1}}</th>
                                            <th>{{(dashboard.resumo.tmp_falado + dashboard.resumo.tmp_aftercall) / dashboard.resumo.tmp_disponivel | percentage:1}}</th>
                                            <th>{{dashboard.resumo.tmp_pausa / dashboard.resumo.tmp_logado | percentage:1}}</th>
                                            <th>{{dashboard.resumo.tmp_ocioso / dashboard.resumo.tmp_disponivel | percentage:1}}</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--PRODUÇÃO POR FILA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Discador/Fila</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table3')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.discador">
                                <thead>
                                    <tr>
                                        <th>Discador</th>
                                        <th>OP</th>
                                        <th>Disc</th>
                                        <th>Att</th>
                                        <th>% Cart.</th>
                                        <th>% Parc.</th>
                                        <th>TMA</th>
                                        <th>TME</th>
                                        <th>% Ocupação</th>
                                        <th>% HitRate</th>
                                        <th>% Ocioso</th>
                                        <th>% CPC</th>
                                        <th>% CPCA</th>
                                        <th>Conversão</th>
                                        <th>Ctt OP</th>
                                        <th>Cpc OP</th>
                                        <th>PROM OP</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in dashboard.discador | groupBy: 'ds_discador'">

                                    <tr class="text-bold">
                                        <td>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{c[0].ds_discador}}
                                        </td>
                                        <td>{{c | sumByColumn: 'qtd_op'| number}}</td>
                                        <td>{{c | sumByColumn: 'discagens'| number}}</td>
                                        <td>{{c | sumByColumn: 'atendidas'| number}}</td>
                                        <td>{{c | divideSumColumns: 'promessa_cartao':'promessa' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa_parc':'promessa' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'tmp_falado':'contato' | formatTimer }}</td>
                                        <td>{{c | divideSumColumns: 'tmp_ocioso':'contato' | formatTimer }}</td>
                                        <td>{{c | divideSumColumns: 'tmp_faladoAftercall':'tmp_disponivel' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'atendidas':'discagens' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'tmp_ocioso':'tmp_disponivel' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'cpca':'cpc' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'contato':'qtd_op' | number:1}}</td>
                                        <td>{{c | divideSumColumns: 'cpc':'qtd_op' | number:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa':'qtd_op' | number:1}}</td>
                                    </tr>

                                    <tr ng-repeat="p in c" ng-show="showTbody">
                                        <td>{{p.ds_campanha}}</td>
                                        <td>{{p.qtd_op | number}}</td>
                                        <td>{{p.discagens | number}}</td>
                                        <td>{{p.atendidas | number}}</td>
                                        <td>{{p.promessa_cartao / p.promessa | percentage:1}}</td>
                                        <td>{{p.promessa_parc / p.promessa | percentage:1}}</td>
                                        <td>{{p.tmp_falado / p.contato | formatTimer}}</td>
                                        <td>{{p.tmp_ocioso / p.contato | formatTimer}}</td>
                                        <td>{{p.tmp_faladoAftercall / p.tmp_disponivel | percentage:1}}</td>
                                        <td>{{p.atendidas / p.discagens | percentage:1}}</td>
                                        <td>{{p.tmp_ocioso / p.tmp_disponivel | percentage:1}}</td>
                                        <td>{{p.cpc / p.contato | percentage:1}}</td>
                                        <td>{{p.cpca / p.cpc | percentage:1}}</td>
                                        <td>{{p.promessa / p.cpca | percentage:1}}</td>
                                        <td>{{p.contato / p.qtd_op | number:1}}</td>
                                        <td>{{p.cpc / p.qtd_op | number:1}}</td>
                                        <td>{{p.promessa / p.qtd_op | number:1}}</td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--PRODUÇÃO POR CARTEIRA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Carteira</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table2')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.carteira">
                                <thead>
                                    <tr>
                                        <th>Carteira</th>
                                        @*<th>Discado</th>*@
                                        @*<th>Atendido</th>*@
                                        <th>Ctt</th>
                                        <th>CPC</th>
                                        <th>CPCA</th>
                                        <th>Prom. </th>
                                        <th>Pro. Cartão</th>
                                        <th>Pro. Migração</th>
                                        <th>Pro. Mg.Cartão</th>
                                        <th>Pro. Parc</th>
                                        <th>% Cartão</th>
                                        <th>% Migração</th>
                                        <th>% Parc</th>
                                        @*<th>Hit Rate</th>
                                            <th>Tx Atd</th>*@
                                        <th>Tx Loc</th>
                                        <th>Tx Apv</th>
                                        <th>Conversão</th>
                                    </tr>
                                </thead>
