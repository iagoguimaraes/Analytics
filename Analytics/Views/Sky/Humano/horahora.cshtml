        <!--PRODUÇÃO POR FILA-->
        <div class="row margin-bottom">
            <div class="col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <span class="card-header-title">Análise por Discador/Fila</span>
                        <div class="card-header-button card-header-expand">
                            <i class="fa fa-expand"></i>
                        </div>
                        <div class="card-header-button" ng-click="extrair('Table8')">
                            <i class="fa fa-file-excel-o"></i>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" style="width:100%" ng-if="dashboard.equipe">
                                <thead>
                                    <tr>
                                        <th>Discador</th>
                                        <th>OP</th>
                                        <th>Disc</th>
                                        <th>Att</th>
                                        <th>CTT</th>
                                        <th>PROM</th>
                                        <th>TMA</th>
                                        <th>TME</th>
                                        <th>% Ocup.</th>
                                        <th>% HitRate</th>
                                        <th>% Impro x Ctt</th>
                                        <th>% Cart.</th>
                                        @*<th>% Migr.</th>*@
                                        <th>% Parc.</th>
                                        <th>% CPC x CTT</th>
                                        <th>% CPCA x CTT</th>
                                        <th>% Aban x CTT</th>
                                        <th>% Desco. x CTT</th>
                                        <th>Conversão</th>
                                        <th>PROM OP</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in dashboard.discador | groupBy: 'discador'">

                                    <tr class="text-bold">
                                        <td>
                                            <i class="fa fa margin-right-5" ng-click="showTbody = !showTbody" ng-class="showTbody ? 'fa-minus' : 'fa-plus'"></i>
                                            {{c[0].discador}}
                                        </td>
                                        <td>{{c | sumByColumn: 'qtd_operador'| number}}</td>
                                        <td>{{c | sumByColumn: 'discagens'| number}}</td>
                                        <td>{{c | sumByColumn: 'atendidas'| number}}</td>
                                        <td>{{c | sumByColumn: 'contato'| number}}</td>
                                        <td>{{c | sumByColumn: 'promessa'| number}}</td>
                                        <td>{{c | sumByColumn: 'tmp_falado'| formatTimer}}</td>
                                        <td>{{c | sumByColumn: 'tmp_espera'| formatTimer}}</td>
                                        <td>{{c | divideSumColumns: 'tmp_falado_total':'tmp_logado_parcial' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'atendidas':'discagens' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'improdutivo':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa_cartao':'promessa' | percentage:1}}</td>
                                        @*<td>{{c | divideSumColumns: 'migracao':'promessa' | percentage:1}}</td>*@
                                        <td>{{c | divideSumColumns: 'promessa_parc':'promessa' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'cpc':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'cpca':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'abandono':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'desconhecido':'contato' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa':'cpca' | percentage:1}}</td>
                                        <td>{{c | divideSumColumns: 'promessa':'qtd_operador' | number:1}}</td>
                                    </tr>

                                    <tr ng-repeat="p in c" ng-show="showTbody">
                                        <td>{{p.fila}}</td>
                                        <td>{{p.qtd_operador | number}}</td>
                                        <td>{{p.discagens | number}}</td>
                                        <td>{{p.atendidas | number}}</td>
                                        <td>{{p.contato | number}}</td>
                                        <td>{{p.promessa | number}}</td>
                                        <td>{{p.tmp_falado | formatTimer}}</td>
                                        <td>{{p.tmp_espera | formatTimer}}</td>
                                        <td>{{p.tmp_falado_total / p.tmp_logado_parcial | percentage:1}}</td>
                                        <td>{{p.atendidas / p.discagens | percentage:1}}</td>
                                        <td>{{p.improdutivo / p.contato  | percentage:1}}</td>
                                        <td>{{p.promessa_cartao / p.promessa | percentage:1}}</td>
                                        @*<td>{{p.migracao / p.promessa | percentage:1}}</td>*@
                                        <td>{{p.promessa_parc / p.promessa | percentage:1}}</td>
                                        <td>{{p.cpc / p.contato | percentage:1}}</td>
                                        <td>{{p.cpca / p.contato | percentage:1}}</td>
                                        <td>{{p.abandono / p.contato | percentage:1}}</td>
                                        <td>{{p.desconhecido / p.contato | percentage:1}}</td>
                                        <td>{{p.promessa / p.cpca | percentage:1}}</td>
                                        <td>{{p.promessa / p.qtd_operador | number:1}}</td>
                                    </tr>

                                </tbody>

                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--PRODUÇÃO POR CARTEIRA-->
