                                <label class="control-sidebar-subheading">Ocorrencia</label>
                                <select2 class="form-control" data-placeholder="Selecione" ng-model="filtros.ocorrencia" dados="dashFiltros.ocorrencia" style="width:100%"></select2>
                            </div>

                            <!--BOTÃO-->
                            <div class="form-group">
                                <button type="button" class="btn btn-primary" data-toggle="control-sidebar" ng-click="carregarDashboard()" onclick="addDisplayNone('filtros-window')">Carregar Dados</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--BANNER-->
        <div class="box box-widget widget-user">
            <div class="widget-user-header" style="background: url('/Content/Images/Sky/bg.png') center center;">
            </div>

